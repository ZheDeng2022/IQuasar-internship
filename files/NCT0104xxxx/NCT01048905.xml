  <brief_title>Glutamine Therapy for Hemolysis-Associated Pulmonary Hypertension</brief_title>
  <official_title>Phase 2 Trial for Glutamine Therapy for Hemolysis-Associated Pulmonary Hypertension</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>UCSF Benioff Children's Hospital Oakland</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
