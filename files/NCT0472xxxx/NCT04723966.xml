  <primary_outcome>
    <measure>electrolyte and mineral disturbance associated with TPN in preterm receiving it</measure>
    <time_frame>14 days</time_frame>
    <description>NA, K, MG, P, CA.</description>
  </primary_outcome>
  <secondary_outcome>
