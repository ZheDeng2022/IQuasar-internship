  <brief_title>Mail Order Mifepristone Study</brief_title>
  <official_title>Feasibility and Acceptability of Dispensing Mifepristone Via Mail Order Pharmacy</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>University of California, San Francisco</agency>
      <agency_class>Other</agency_class>
