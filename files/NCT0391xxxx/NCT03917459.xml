  <primary_outcome>
    <measure>Erectile function score at 3 months</measure>
    <time_frame>3 months</time_frame>
    <description>The questionnaire International Index of Erectile Function (IIEF-15) is used to assess erectile function</description>
  </primary_outcome>
  <secondary_outcome>
