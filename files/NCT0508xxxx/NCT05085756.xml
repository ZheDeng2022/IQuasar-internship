  <brief_summary>
    <textblock>
      This study will offer transdiagnostic CBT-based treatment for symptoms of depression and&#xD;
      anxiety to university students who have previously responded to the WHO-WMH-ICS survey.&#xD;
    </textblock>
  </brief_summary>
  <overall_status>Not yet recruiting</overall_status>
  <start_date type="Anticipated">October 18, 2021</start_date>
  <completion_date type="Anticipated">December 31, 2024</completion_date>
  <primary_completion_date type="Anticipated">October 31, 2023</primary_completion_date>
  <phase>N/A</phase>
  <study_type>Interventional</study_type>
  <has_expanded_access>No</has_expanded_access>
  <study_design_info>
    <allocation>Randomized</allocation>
    <intervention_model>Parallel Assignment</intervention_model>
    <primary_purpose>Treatment</primary_purpose>
    <masking>Single (Participant)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>PHQ-9 for depression</measure>
    <time_frame>last 2 weeks</time_frame>
    <description>9 items measuring depressive symptoms</description>
  </primary_outcome>
  <primary_outcome>
    <measure>GAD-7 for anxiety</measure>
    <time_frame>last 2 weeks</time_frame>
    <description>7 items measuring anxiety symptoms</description>
  </primary_outcome>
  <number_of_arms>3</number_of_arms>
  <enrollment type="Anticipated">200</enrollment>
  <condition>Psychological Disorder</condition>
  <arm_group>
    <arm_group_label>Guided treatment</arm_group_label>
    <arm_group_type>Active Comparator</arm_group_type>
    <description>8-week transdiagnostic CBT with written guidance from M.Sc-level students under supervision.</description>
  </arm_group>
  <arm_group>
    <arm_group_label>Unguided treatment</arm_group_label>
    <arm_group_type>Active Comparator</arm_group_type>
    <description>8-week transdiagnostic CBT without guidance.</description>
  </arm_group>
  <arm_group>
    <arm_group_label>Waitlist</arm_group_label>
    <arm_group_type>Other</arm_group_type>
    <description>8-week transdiagnostic CBT without guidance, made available 6 months after recruitment.</description>
  </arm_group>
  <intervention>
    <intervention_type>Behavioral</intervention_type>
    <intervention_name>Puma</intervention_name>
    <description>One psychoeducation module followed by two transdiagnostic modules tailored to primary depression or anxiety symptoms, followed by three participant-choice modules, and ending with two transdiagnostic modules.</description>
    <arm_group_label>Guided treatment</arm_group_label>
    <arm_group_label>Unguided treatment</arm_group_label>
    <arm_group_label>Waitlist</arm_group_label>
  </intervention>
  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria:&#xD;
&#xD;
          -  PHQ-9 cutoff for mild depression&#xD;
&#xD;
          -  GAD-7 cutoff for mild anxiety&#xD;
&#xD;
        Exclusion Criteria:&#xD;
&#xD;
          -  Suicidal ideation or plans&#xD;
&#xD;
          -  Severe levels of mental ill-health&#xD;
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>N/A</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>
