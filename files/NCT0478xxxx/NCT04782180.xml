  <brief_title>PrEP at a Syringe Services Program</brief_title>
  <official_title>Mobile Delivery of PrEP at a Syringe Services Program-A Pilot Study</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>University of Miami</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
