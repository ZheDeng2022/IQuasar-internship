  <condition>Ovarian Epithelial Cancer</condition>
  <arm_group>
    <arm_group_label>Olaparib</arm_group_label>
    <arm_group_type>Experimental</arm_group_type>
    <description>olaparib 300mg oral tablets twice daily for 28 days in 28-day cycles</description>
  </arm_group>
  <arm_group>
