  <brief_title>Confirmatory Study of OPC-12759 Ophthalmic Solution</brief_title>
  <official_title>Confirmatory Study of OPC-12759 Ophthalmic Solution in Patients With Dry Eye</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Otsuka Pharmaceutical Co., Ltd.</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
