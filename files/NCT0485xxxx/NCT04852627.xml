  <brief_title>Effect of Brisk Walking on Primary Dysmenorrhea</brief_title>
  <official_title>Effect of Brisk Walking on Primary Dysmenorrhea Among Medical Students</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Ain Shams University</agency>
