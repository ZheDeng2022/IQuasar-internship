    <masking>Double (Participant, Investigator)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Change in fasting triglycerides</measure>
    <time_frame>Baseline, Week 4 and Week 8</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Change in total cholesterol, LDL-C, HDL-C</measure>
    <time_frame>Baseline, Week 4 and Week 8</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Change in BMI (kg/m2)</measure>
    <time_frame>Baseline, Week 4 and Week 8</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Change in Waist circumference (cm)</measure>
    <time_frame>Baseline, Week 4 and Week 8</time_frame>
  </secondary_outcome>
  <number_of_arms>2</number_of_arms>
  <enrollment type="Actual">100</enrollment>
  <condition>Triglycerides High</condition>
  <arm_group>
    <arm_group_label>Heukcha Extracts</arm_group_label>
    <arm_group_type>Experimental</arm_group_type>
