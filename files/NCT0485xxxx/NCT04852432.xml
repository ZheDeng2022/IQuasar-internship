    <arm_group_type>Experimental</arm_group_type>
    <description>Oxygen is administered at a rate of 2L/kg/min using an Optiflow device</description>
  </arm_group>
  <intervention>
    <intervention_type>Device</intervention_type>
    <intervention_name>high flow nasal cannula</intervention_name>
    <description>The heated air is administered at a rate of 2L/kg/min using an Optiflow device (Fisher and Paykel Healthcare, Auckland, New Zealand). Inhalation oxygen concentration starts with 50%.</description>
    <arm_group_label>High flow group</arm_group_label>
  </intervention>
  <intervention>
    <intervention_type>Device</intervention_type>
    <intervention_name>nasal prong</intervention_name>
