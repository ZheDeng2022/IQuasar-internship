    <time_perspective>Prospective</time_perspective>
  </study_design_info>
  <condition>Uterine Fibroids</condition>
  <condition>Menorrhagia</condition>
  <condition>Adenomyosis</condition>
  <intervention>
    <intervention_type>Procedure</intervention_type>
    <intervention_name>Uterine Artery Embolization</intervention_name>
  </intervention>
  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria:&#xD;
&#xD;
          -  Symptoms related to the pressence of uterine fibroids serious enough to consider&#xD;
             surgical options&#xD;
&#xD;
          -  Confirmation of fibroids by endovaginal and transabdominal ultrasound&#xD;
&#xD;
        Exclusion Criteria:&#xD;
&#xD;
          -  Pedunculated fibroids&#xD;
&#xD;
          -  Desire for fertility&#xD;
&#xD;
          -  Carcinoma of pelvic organs&#xD;
&#xD;
          -  Previous pelvic irradiation&#xD;
&#xD;
          -  Bleeding diathesis and vasculitis&#xD;
&#xD;
          -  History of allergiy to contrst medium&#xD;
      </textblock>
    </criteria>
    <gender>Female</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>60 Years</maximum_age>
  </eligibility>
