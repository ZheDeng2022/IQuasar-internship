  <brief_title>Metformin for the Prevention of the Metabolic Side-effects of Zyprexa</brief_title>
  <official_title>Metformin to Prevent the Metabolic Complications of Olanzapine</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Rush University Medical Center</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
