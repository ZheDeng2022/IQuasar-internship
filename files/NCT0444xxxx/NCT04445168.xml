  <brief_title>Randomized Trial of Exercise Promotion in Primary Care</brief_title>
  <acronym>EPPC</acronym>
  <official_title>Intervention Based in Primary Care to Increase Physical Activity Among Inactive Adults With Prediabetes and Diabetes</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Kaiser Permanente</agency>
      <agency_class>Other</agency_class>
