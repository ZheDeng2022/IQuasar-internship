  <primary_outcome>
    <measure>Fecal microbiota</measure>
    <time_frame>Changes from baseline to 10 weeks of intervention</time_frame>
    <description>Abundance of bacterial taxa determined by 16SRNAr gene sequencing</description>
  </primary_outcome>
  <primary_outcome>
    <measure>Fecal metabolome profile</measure>
    <time_frame>Changes from baseline to 10 weeks of intervention</time_frame>
