  <brief_title>Effect of Cocoa Supplementation in Training Athletes Microbiota and Metabolome</brief_title>
  <official_title>Effect of Cocoa Supplementation in Training Athletes Microbiota and Metabolome: Effects on Acute and Adaptive Exercise</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Universidad Europea de Madrid</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
