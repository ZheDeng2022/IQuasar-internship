  <brief_title>Exjade-Early-Trial</brief_title>
  <official_title>Early Treatment With Deferasirox (Exjade®) in Low Risk MDS - a Prospective Multicentre Single-arm Single-stage Phase II Study -</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>University of Erlangen-Nürnberg Medical School</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
