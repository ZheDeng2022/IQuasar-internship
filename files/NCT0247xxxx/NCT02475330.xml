  <brief_title>Dietary Flaxseed in NSCLC</brief_title>
  <official_title>Phase II Trial of Flaxseed to Prevent Pneumonopathy After Chemoradiation for Lung Cancer</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Abramson Cancer Center of the University of Pennsylvania</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
