  <brief_title>A Dose Ranging Placebo-Controlled Double-Blind Study to Evaluate the Safety and Efficacy of Tezepelumab in Atopic Dermatitis</brief_title>
  <official_title>A Dose-Ranging, Double-Blind, Placebo-Controlled Study to Evaluate the Safety and Efficacy of Tezepelumab Alone or Combined With Topical Corticosteroids in Moderate-to-Severe Atopic Dermatitis</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Amgen</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
    <collaborator>
      <agency>AstraZeneca</agency>
      <agency_class>Industry</agency_class>
    </collaborator>
  </sponsors>
