  <brief_title>Natural History Clinical Study in Adult PKU</brief_title>
  <official_title>A Prospective Study Investigating the Natural History of Adults With Phenylketonuria (PKU) Due to Phenylalanine Hydroxylase Deficiency</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Homology Medicines, Inc</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
