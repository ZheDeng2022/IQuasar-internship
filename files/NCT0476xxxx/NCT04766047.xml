  <brief_title>The Effect of Dexmedetomidine on Kidney Function in EVAR</brief_title>
  <acronym>DEVAR</acronym>
  <official_title>The Effect of Dexmedetomidine on the Incidence of Acute Kidney Injury in EVAR</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>University of Thessaly</agency>
      <agency_class>Other</agency_class>
