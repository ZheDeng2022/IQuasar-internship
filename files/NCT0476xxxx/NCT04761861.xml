  <brief_title>Vildagliptin for Treatment of Antipsychotic-induced Dyslipidemia</brief_title>
  <official_title>Vildagliptin for Treatment of Antipsychotic-induced Dyslipidemia: a Randomized, Double-blind, Placebo-controlled Study</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Sadat City University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
