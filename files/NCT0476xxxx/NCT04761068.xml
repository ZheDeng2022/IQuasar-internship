  </secondary_outcome>
  <secondary_outcome>
    <measure>Recurrent revascularization</measure>
    <time_frame>1, 2, and 5 years post index CABG</time_frame>
    <description>Recurrent revascularization</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Survival</measure>
    <time_frame>1, 2, and 5 years post index CABG</time_frame>
    <description>Survival, both overall and freedom from cardiac death</description>
  </secondary_outcome>
