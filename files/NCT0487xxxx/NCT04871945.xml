  <brief_title>Protective Antibody Level After COVID-19 Vaccination Among Patients Under Hemodialysis</brief_title>
  <official_title>Evaluation of Protective Antibody Production After COVID-19 Vaccination Among Patients Under Hemodialysis</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Hanyang University Seoul Hospital</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
