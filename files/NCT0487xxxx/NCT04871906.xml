  <brief_title>The Effect of Stretching Exercises on Vascular Function, Blood Flow, and Walking Function in Middle and Older Adults</brief_title>
  <official_title>The Effect of Stretching Exercises on Vascular Function, Blood Flow, and Walking Function in Middle and Older Adults</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>National Yang Ming University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
  <source>National Yang Ming University</source>
  <oversight_info>
