  </arm_group>
  <intervention>
    <intervention_type>Drug</intervention_type>
    <intervention_name>AT-527</intervention_name>
    <description>administered twice daily (BID) for 2.5 days (5 doses in total)</description>
    <arm_group_label>AT-527 Group A</arm_group_label>
    <arm_group_label>AT-527 Group B</arm_group_label>
    <arm_group_label>AT-527 Group C</arm_group_label>
