  </study_design_info>
  <primary_outcome>
    <measure>Decrease in intraocular pressure (IOP)</measure>
    <time_frame>12 months</time_frame>
    <description>Decrease in unmedicated mean diurnal IOP (DIOP)</description>
  </primary_outcome>
  <primary_outcome>
    <measure>Decrease in number of medications</measure>
    <time_frame>12 months</time_frame>
    <description>Mean change in the number of ocular hypotensive medications.</description>
  </primary_outcome>
  <number_of_groups>2</number_of_groups>
  <enrollment type="Anticipated">100</enrollment>
  <condition>Glaucoma, Open-Angle</condition>
  <arm_group>
