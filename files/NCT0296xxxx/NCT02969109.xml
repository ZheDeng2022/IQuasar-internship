  </study_design_info>
  <primary_outcome>
    <measure>The recurrence status based on the pathology results</measure>
    <time_frame>1 year</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>The Genomic Health bladder cancer assay results.</measure>
    <time_frame>1 year</time_frame>
  </primary_outcome>
  <enrollment type="Actual">417</enrollment>
  <condition>Non-muscle Invasive Bladder Cancer</condition>
  <biospec_retention>Samples Without DNA</biospec_retention>
