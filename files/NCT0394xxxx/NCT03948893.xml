  <brief_title>Smoking Cessation Behavioral Treatment Study</brief_title>
  <official_title>Neural Mechanisms Mediating Appetitive Regulation and Smoking in Nicotine Addiction</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>University of Missouri-Columbia</agency>
      <agency_class>Other</agency_class>
