  <brief_title>Surgery Versus Radiotherapy for Locally Advanced Prostate Cancer</brief_title>
  <acronym>SPCG-15</acronym>
  <official_title>Primary Radical Prostatectomy Versus Primary Radiotherapy for Locally Advanced Prostate Cancer: an Open Randomized Clinical Trial</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Olof Akre</agency>
      <agency_class>Other</agency_class>
