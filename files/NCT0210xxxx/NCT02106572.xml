  <brief_title>Therapeutic Instillation of Mistletoe</brief_title>
  <acronym>TIM</acronym>
  <official_title>Intravesical Mistletoe Extract in Superficial Bladder Cancer: A Phase III Efficacy Study</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Abnoba Gmbh</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
  <source>Abnoba Gmbh</source>
  <oversight_info>
