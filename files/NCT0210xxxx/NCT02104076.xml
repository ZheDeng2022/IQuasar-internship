  <brief_title>Evolution® Biliary Stent System-Fully Covered</brief_title>
  <official_title>Evaluation for the Effectiveness of Evolution® Biliary Stent System-Fully Covered</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Cook Group Incorporated</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
