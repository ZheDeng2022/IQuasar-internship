    <masking>None (Open Label)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Percentage of patients free from symptomatic recurrent of biliary obstruction requiring reintervention</measure>
    <time_frame>6 months</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Total Serum Bilirubin ≤ 3.0 mg/dL</measure>
    <time_frame>1 month</time_frame>
  </primary_outcome>
  <number_of_arms>1</number_of_arms>
  <enrollment type="Actual">90</enrollment>
