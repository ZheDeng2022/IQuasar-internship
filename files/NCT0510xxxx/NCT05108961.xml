  <primary_outcome>
    <measure>Bispectral Index</measure>
    <time_frame>35 minutes</time_frame>
    <description>Bispectral Index (BIS) measured every second for 35 min through an EEG</description>
  </primary_outcome>
  <number_of_arms>3</number_of_arms>
