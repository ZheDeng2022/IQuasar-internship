  <primary_outcome>
    <measure>Cognition</measure>
    <time_frame>6 months</time_frame>
    <description>test attention with STROOP</description>
  </primary_outcome>
  <primary_outcome>
    <measure>Cognition</measure>
    <time_frame>6 months</time_frame>
    <description>Test concentration with 'cijferdoorstreep test'</description>
  </primary_outcome>
  <primary_outcome>
    <measure>Cognition</measure>
    <time_frame>6 months</time_frame>
    <description>Test psychomotor speed with grooved pegboard</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>vascular structure</measure>
    <time_frame>6 months</time_frame>
    <description>measure vascular structure with ultrasound.</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Mood</measure>
    <time_frame>6 months</time_frame>
    <description>Test mood with visual analogue mood scale. 8 emotions are scored on a 1-10 scale.</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Blood pressure</measure>
    <time_frame>6 months</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Liver function</measure>
    <time_frame>6 months</time_frame>
    <description>analysis of alkaline phosphatase, alanine aminotransferase, aspartaat aminotransferase, bilirubine, gamma-glutamyltransferase to determine liver function.</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>flavo-enzyme</measure>
    <time_frame>6 months</time_frame>
    <description>monoamine oxidase B analysis</description>
  </secondary_outcome>
