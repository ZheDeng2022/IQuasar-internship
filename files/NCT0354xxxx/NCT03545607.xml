  <brief_title>MultiStem® Administration for Stroke Treatment and Enhanced Recovery Study</brief_title>
  <acronym>MASTERS-2</acronym>
  <official_title>MultiStem® Administration for Stroke Treatment and Enhanced Recovery Study (MASTERS-2)</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Athersys, Inc</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
