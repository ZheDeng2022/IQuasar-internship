  </study_design_info>
  <primary_outcome>
    <measure>Progression-free survival (PFS)</measure>
    <time_frame>8 months</time_frame>
  </primary_outcome>
  <enrollment type="Anticipated">200</enrollment>
  <condition>Cancer of Unknown Primary Site</condition>
  <intervention>
