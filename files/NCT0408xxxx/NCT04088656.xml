  <primary_outcome>
    <measure>Blood pressure screening</measure>
    <time_frame>year 2 through year 4</time_frame>
    <description>The proportion of routine care visits by people living with HIV at which blood pressure is measured</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Hypertension diagnosis</measure>
    <time_frame>year 2 through year 4</time_frame>
    <description>The proportion of confirmed diagnoses of hypertension amongst routine care visits of HIV patients</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Hypertension treatment initiation</measure>
    <time_frame>year 2 through year 4</time_frame>
    <description>The proportion of hypertension medication-eligible HIV patients initiating hypertension treatment</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Hypertension medication pick up</measure>
    <time_frame>year 2 through year 4</time_frame>
