  <brief_title>Network Analysis of Adult MDD and ADHD</brief_title>
  <official_title>Network Analysis of Co-morbid Symptoms of Adult Major Depressive Disorder and Attention-deficit Hyperactivity Disorder in an Outpatient Population</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Health Sciences North Research Institute</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
  <source>Health Sciences North Research Institute</source>
  <oversight_info>
    <has_dmc>No</has_dmc>
