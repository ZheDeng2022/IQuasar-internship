  <brief_title>Self-monitoring Focus Groups With Vibrant Lives Pasadena Participants</brief_title>
  <official_title>Self-Monitoring Focus Groups With Vibrant Lives Pasadena Participants</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>M.D. Anderson Cancer Center</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
