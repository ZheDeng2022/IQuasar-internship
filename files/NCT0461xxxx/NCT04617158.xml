  <brief_title>Comparing Adjustable Sutures Surgery</brief_title>
  <official_title>Comparison Between Immediate Versus Late Postoperative Adjustable Eye Muscle Surgery</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Minia University</agency>
      <agency_class>Other</agency_class>
