  <brief_title>Fall Prevention Study Among Seniors in Bomlo</brief_title>
  <official_title>Early Identification and Intervention in Risk of Falling Among Seniors i Bomlo Municipality</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Kjersti Helen Folleso</agency>
      <agency_class>Other</agency_class>
