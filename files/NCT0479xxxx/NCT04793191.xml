  <brief_title>Predicting Acute Postoperative Pain by the Preoperative Lower Back Pain</brief_title>
  <official_title>Predicting Acute Postoperative Pain by the Preoperative Lower Back Pain in the Gynecological Laparoscopy</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>The Catholic University of Korea</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
