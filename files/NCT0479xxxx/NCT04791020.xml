  </study_design_info>
  <primary_outcome>
    <measure>Global evaluation of pain</measure>
    <time_frame>5 minutes post procedure</time_frame>
    <description>Visible scale of pain evaluation</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Adverse reaction</measure>
    <time_frame>5 minutes post procedure</time_frame>
    <description>Presence of adverse effects to the drug used (lidocaine gel)</description>
  </secondary_outcome>
  <number_of_arms>2</number_of_arms>
  <enrollment type="Actual">100</enrollment>
  <condition>Pain</condition>
