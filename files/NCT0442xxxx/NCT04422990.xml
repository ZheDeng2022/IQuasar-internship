  <brief_title>Clinical Evaluation of a Silicone Hydrogel Daily Wear Monthly Replacement Contact Lens</brief_title>
  <official_title>Clinical Evaluation of a Silicone Hydrogel Daily Wear Monthly Replacement Contact Lens</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Alcon Research</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
