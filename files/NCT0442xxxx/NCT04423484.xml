  <eligibility>
    <study_pop>
      <textblock>
        All children with local Nephroblastoma in all participating units&#xD;
      </textblock>
    </study_pop>
    <sampling_method>Non-Probability Sample</sampling_method>
    <criteria>
      <textblock>
        Inclusion Criteria:&#xD;
&#xD;
        Unilateral Nephroblastoma Tumor Not previously treated The general health of the child will&#xD;
        permit treatment.&#xD;
&#xD;
        .&#xD;
&#xD;
        Exclusion Criteria:&#xD;
&#xD;
        Bilateral Nephroblastoma tumor Previously treated Disease too advanced Doubt concerning the&#xD;
        diagnosis Treatment Refusal&#xD;
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>6 Months</minimum_age>
    <maximum_age>18 Years</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
