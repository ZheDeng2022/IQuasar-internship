  <brief_title>Alcohol and Immune Response</brief_title>
  <official_title>Immune Activation and Neurodegeneration in HIV Infection and Heavy Drinking</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Brown University</agency>
