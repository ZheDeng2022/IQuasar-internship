  <brief_title>Effect of Electroacupuncture on P6 Acupoint on Hypertensive Patients.</brief_title>
  <acronym>EEP6</acronym>
  <official_title>Effect of Electroacupuncture on P6 Acupoint on Hypertensive Patients.</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Ong Fujian Chinese Physician Hall</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
