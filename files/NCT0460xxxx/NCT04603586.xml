    <arm_group_label>Arm A</arm_group_label>
    <arm_group_type>Experimental</arm_group_type>
    <description>SBRT with BED 60-70Gy combined with Gemcitabine + albumin-bound paclitaxel</description>
  </arm_group>
  <arm_group>
    <arm_group_label>Arm B</arm_group_label>
    <arm_group_type>Experimental</arm_group_type>
