  <brief_title>Observational Epidemiologic Study in Women With Premature Ovarian Failure (POF)</brief_title>
  <official_title>Observational Epidemiologic Study in Women With Premature Ovarian Failure (POF)</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Fundació Institut de Recerca de l'Hospital de la Santa Creu i Sant Pau</agency>
      <agency_class>Other</agency_class>
