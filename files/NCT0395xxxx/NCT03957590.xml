  <start_date type="Actual">June 12, 2019</start_date>
  <completion_date type="Anticipated">October 30, 2023</completion_date>
  <primary_completion_date type="Anticipated">November 30, 2021</primary_completion_date>
  <phase>Phase 3</phase>
  <study_type>Interventional</study_type>
  <has_expanded_access>No</has_expanded_access>
