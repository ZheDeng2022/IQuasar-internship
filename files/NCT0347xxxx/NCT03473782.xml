  <primary_outcome>
    <measure>Measure of urgency incontinence using Voiding Diary</measure>
    <time_frame>60 Months</time_frame>
    <description>patients will record urgency in diary</description>
  </primary_outcome>
  <number_of_groups>2</number_of_groups>
