  <oversight_info>
    <has_dmc>No</has_dmc>
  </oversight_info>
  <brief_summary>
    <textblock>
      To determine if ultrasound measurement of renal and carotid arterial resistive indices are&#xD;
      associated with post-cardiac surgery acute kidney injury.&#xD;
    </textblock>
  </brief_summary>
  <overall_status>Recruiting</overall_status>
  <start_date>October 2015</start_date>
  <completion_date type="Anticipated">September 2023</completion_date>
  <primary_completion_date type="Anticipated">September 2023</primary_completion_date>
  <study_type>Observational</study_type>
  <has_expanded_access>No</has_expanded_access>
  <study_design_info>
    <observational_model>Cohort</observational_model>
    <time_perspective>Prospective</time_perspective>
  </study_design_info>
  <primary_outcome>
    <measure>Acute kidney injury - Lassnigg grade-minimal</measure>
    <time_frame>2nd postoperative day</time_frame>
  </primary_outcome>
  <enrollment type="Anticipated">125</enrollment>
  <condition>Cardiac Surgical Procedures</condition>
  <eligibility>
    <study_pop>
      <textblock>
        Adult patients undergoing cardiac surgery&#xD;
      </textblock>
    </study_pop>
    <sampling_method>Non-Probability Sample</sampling_method>
    <criteria>
      <textblock>
        Inclusion Criteria:&#xD;
&#xD;
          -  adult patients &gt;= 18 years&#xD;
&#xD;
          -  undergoing cardiac surgery with cardiopulmonary bypass and transesophageal&#xD;
             echocardiography (TEE) probe placement&#xD;
&#xD;
        Exclusion Criteria:&#xD;
&#xD;
          -  chronic kidney diseases (estimated GFR &lt; 60 ml/min/1.73m2)&#xD;
&#xD;
          -  prior carotid endarterectomy&#xD;
&#xD;
          -  known carotid artery stenosis of 50% or greater&#xD;
&#xD;
          -  dissection or aneurysm involving renal or carotid arteries&#xD;
&#xD;
          -  scheduled aortic or aortic valve surgery&#xD;
&#xD;
          -  heart transplant&#xD;
&#xD;
          -  intraaortic balloon pump, ventricular assist device, or exctracorporeal membrane&#xD;
             oxygenation support.&#xD;
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>N/A</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>
