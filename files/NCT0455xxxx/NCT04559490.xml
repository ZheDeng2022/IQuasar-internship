  <primary_outcome>
    <measure>mean change in blood glucose</measure>
    <time_frame>baseline to 1 hour</time_frame>
    <description>change in blood glucose from fasting to 1 hour post glucose challenge</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>mean endogenous insulin secretion</measure>
    <time_frame>baseline to 1 hour</time_frame>
    <description>change in mean insulin collected at baseline and 1 hour post glucose challenge</description>
  </secondary_outcome>
  <number_of_groups>2</number_of_groups>
  <enrollment type="Anticipated">120</enrollment>
  <condition>Pancreas Disease</condition>
  <arm_group>
    <arm_group_label>Standard formula</arm_group_label>
