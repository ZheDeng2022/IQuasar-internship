  <brief_title>Interaction Study of Zanubrutinib With Moderate and Strong CYP3A Inhibitors in Participants With B-Cell Malignancies</brief_title>
  <official_title>A Drug-Drug Interaction Study of Zanubrutinib With Moderate and Strong CYP3A Inhibitors in Patients With B-Cell Malignancies</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>BeiGene</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
  <source>BeiGene</source>
  <oversight_info>
    <has_dmc>Yes</has_dmc>
    <is_fda_regulated_drug>Yes</is_fda_regulated_drug>
