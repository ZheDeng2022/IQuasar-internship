  <brief_title>Microneedles for Diagnosis of LTBI</brief_title>
  <official_title>Pilot Study for Development of Microneedles for Diagnosis of Latent Tuberculosis Infection</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>The HIV Netherlands Australia Thailand Research Collaboration</agency>
      <agency_class>Other</agency_class>
