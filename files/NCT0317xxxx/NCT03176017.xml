  <brief_title>Ejaculatory Sparing Transurethral Incision Of The Prostate (ES-TUIP) Versus Conventional TUIP</brief_title>
  <official_title>Ejaculatory Sparing Transurethral Incision Of The Prostate (ES-TUIP) Versus Conventional TUIP</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Mansoura University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
