  <brief_title>Chemotherapy and Imatinib in Young Adults With Acute Lymphoblastic Leukemia Ph (BCR-ABL) POSITIVE</brief_title>
  <sponsors>
    <lead_sponsor>
      <agency>PETHEMA Foundation</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
