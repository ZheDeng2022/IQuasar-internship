    <allocation>Randomized</allocation>
    <intervention_model>Parallel Assignment</intervention_model>
    <primary_purpose>Treatment</primary_purpose>
    <masking>None (Open Label)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Progression Free Survival at 6 months</measure>
    <time_frame>6 months after start of maintenance therapy</time_frame>
    <description>Endpoints will include Progression assessed by CT Scan according to RECIST 1.1 criteria and Death from any cause</description>
  </primary_outcome>
  <number_of_arms>2</number_of_arms>
  <enrollment type="Actual">139</enrollment>
  <condition>Metastatic Colorectal Cancer</condition>
