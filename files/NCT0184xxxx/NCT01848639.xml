  <brief_title>ALdosterone Antagonist Chronic HEModialysis Interventional Survival Trial</brief_title>
  <acronym>ALCHEMIST</acronym>
  <official_title>ALdosterone Antagonist Chronic HEModialysis Interventional Survival Trial (ALCHEMIST), Phase III b</official_title>
  <sponsors>
