  </oversight_info>
  <brief_summary>
    <textblock>
      Clinical trial to evaluate the effect of a probiotic containing Lactobacillus Crispatus on&#xD;
      the vaginal flora&#xD;
    </textblock>
  </brief_summary>
  <overall_status>Completed</overall_status>
  <start_date type="Actual">March 21, 2019</start_date>
  <completion_date type="Actual">October 30, 2019</completion_date>
  <primary_completion_date type="Actual">September 30, 2019</primary_completion_date>
  <phase>N/A</phase>
  <study_type>Interventional</study_type>
  <has_expanded_access>No</has_expanded_access>
