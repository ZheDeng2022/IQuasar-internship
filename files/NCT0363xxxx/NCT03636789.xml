  </oversight_info>
  <brief_summary>
    <textblock>
      This study aims to investigate the prevalence of subclinical oculomotor disorders in a&#xD;
      population of patients with radiologically isolated syndrome.&#xD;
    </textblock>
  </brief_summary>
  <overall_status>Active, not recruiting</overall_status>
  <start_date type="Actual">October 26, 2018</start_date>
  <completion_date type="Anticipated">December 2021</completion_date>
  <primary_completion_date type="Actual">October 21, 2021</primary_completion_date>
  <phase>N/A</phase>
  <study_type>Interventional</study_type>
  <has_expanded_access>No</has_expanded_access>
  <study_design_info>
    <allocation>N/A</allocation>
    <intervention_model>Single Group Assignment</intervention_model>
    <primary_purpose>Other</primary_purpose>
    <masking>None (Open Label)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Evaluation of the occurrence of oculomotor disorders</measure>
    <time_frame>Day 0</time_frame>
    <description>using a video-oculographic recording system</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>To establish the prevalence of each of the different types of oculomotor abnormalities that constitute the oculomotor disorder</measure>
    <time_frame>Day 0</time_frame>
    <description>number</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>To identify a correlation between oculomotor disorders and MRI abnormalities.</measure>
    <time_frame>Day 0</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>To identify a correlation between oculomotor disorders and the cognitive impairment test (Symbol Digit Modalities Test)</measure>
    <time_frame>Day 0</time_frame>
  </secondary_outcome>
  <number_of_arms>1</number_of_arms>
  <enrollment type="Actual">61</enrollment>
  <condition>Radiologically Isolated Syndrome (RIS)</condition>
  <condition>Multiple Sclerosis</condition>
  <arm_group>
    <arm_group_label>Neurological consultation group</arm_group_label>
