  <primary_outcome>
    <measure>Glomerular Fitration Rate</measure>
    <time_frame>At 24 months</time_frame>
    <description>Glomerular Fitration Rate estimated by the CKD-EPI formula with a blood sample</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Proteinuria</measure>
    <time_frame>At 24 months</time_frame>
    <description>Proteinuria (g/24h) found in the 24-hour urine</description>
  </secondary_outcome>
