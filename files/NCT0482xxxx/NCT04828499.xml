  <primary_outcome>
    <measure>corneal endothelial cell density</measure>
    <time_frame>up to 1 months</time_frame>
    <description>the number of corneal endothelial cells per unit area.</description>
  </primary_outcome>
  <primary_outcome>
    <measure>Coefficient of variation of endothelial cells</measure>
    <time_frame>up to 1 months</time_frame>
    <description>represents the variation coefficient of cell area</description>
  </primary_outcome>
  <primary_outcome>
    <measure>percentage of hexagonal cells of the cells marked</measure>
    <time_frame>up to 1 months</time_frame>
    <description>represents the percentage of hexagon cells,the ideal value is above 50%</description>
  </primary_outcome>
  <primary_outcome>
    <measure>central corneal thickness</measure>
    <time_frame>up to 1 months</time_frame>
    <description>thickness in micrometer</description>
  </primary_outcome>
