  <brief_title>Diagnostic Errors in Anaphylactic Shock</brief_title>
  <acronym>ANASim</acronym>
  <official_title>Context-dependent Diagnostic Accuracy in Anaphylactic Shock</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>University Hospital, Basel, Switzerland</agency>
