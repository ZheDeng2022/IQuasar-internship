  <brief_title>FlowTriever All-Comer Registry for Patient Safety and Hemodynamics</brief_title>
  <acronym>FLASH</acronym>
  <official_title>FlowTriever All-Comer Registry for Patient Safety and Hemodynamics (FLASH)</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Inari Medical</agency>
