  <brief_title>A Clinical Investigation of the Vanguard™ Complete Knee System</brief_title>
  <official_title>A Prospective, Non-controlled, Clinical Investigation of the Vanguard™ Complete Knee System</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Zimmer Biomet</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
