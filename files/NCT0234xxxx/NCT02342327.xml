  <brief_title>Smoking Cessation in Menthol Flavored Cigarette Smokers</brief_title>
  <sponsors>
    <lead_sponsor>
      <agency>University of Minnesota</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
