  <brief_title>Effect of Atorvastatin on Bone-vascular Axis</brief_title>
  <official_title>Effect of Atorvastatin on OPG/RANK/RANKL Expression in Immune Cells and Circulating Levels of Osteoprogenitor Cells</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>University of Padova</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
  <source>University of Padova</source>
