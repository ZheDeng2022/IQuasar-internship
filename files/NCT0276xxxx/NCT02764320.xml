  <brief_title>The MOTS (Medication Overuse Treatment Strategy) Trial</brief_title>
  <acronym>MOTS</acronym>
  <official_title>Determining the Optimal Treatment Strategy for Patients Who Have Chronic Migraine With Medication Overuse</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Mayo Clinic</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
