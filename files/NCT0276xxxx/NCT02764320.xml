  <primary_outcome>
    <measure>Frequency of Moderate to Severe Headache Days</measure>
    <time_frame>12 weeks</time_frame>
    <description>Number of days on which a headache lasts for at least 2 hours and at any time peaks at moderate or severe intensity</description>
  </primary_outcome>
  <number_of_arms>2</number_of_arms>
  <enrollment type="Actual">720</enrollment>
  <condition>Migraine</condition>
