  <brief_title>Assessment of Potential for Chronic Liver Injury in Participants Treated With Epidiolex (Cannabidiol) Oral Solution</brief_title>
  <official_title>A Long-term Safety Study to Assess the Potential for Chronic Liver Injury in Participants Treated With Epidiolex (Cannabidiol) Oral Solution</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>GW Research Ltd</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
