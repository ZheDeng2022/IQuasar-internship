  <brief_title>Evaluation of BOL-303242-X Versus Vehicle for the Treatment of Inflammation Following Cataract Surgery</brief_title>
  <sponsors>
    <lead_sponsor>
      <agency>Bausch &amp; Lomb Incorporated</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
