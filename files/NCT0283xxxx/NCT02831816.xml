    <outcome_list>
      <outcome>
        <type>Primary</type>
        <title>Bacterial Reduction</title>
        <description>A a 3-log per cm2 bacterial reduction on the inguinal region is considered a success.</description>
        <time_frame>10 minutes post product application</time_frame>
        <population>Treated subjects with treatment day baseline criteria 5.0-7.5 log recoveries.</population>
        <group_list>
          <group group_id="O1">
            <title>Mean Log Reduction - ZP (70% IPA) - Groin</title>
