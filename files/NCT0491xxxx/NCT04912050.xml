  <primary_outcome>
    <measure>Treatment Success</measure>
    <time_frame>Up to 24 hours</time_frame>
    <description>Percentage of treatments achieving prescribed dialysis treatment time ≤ 10%</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Tablo Cartridge Life</measure>
    <time_frame>up to 24 hours</time_frame>
    <description>Measure the average number of cartridge changes required to complete a Tablo XT Treatment</description>
  </secondary_outcome>
