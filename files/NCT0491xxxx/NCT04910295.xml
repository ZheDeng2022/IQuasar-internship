  <brief_title>Immunogenicity and Safety of COVID-19 Vaccine in Cancer Patients</brief_title>
  <official_title>Immunogenicity and Safety of COVID-19 Vaccine in Cancer Patients</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Seoul National University Hospital</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
