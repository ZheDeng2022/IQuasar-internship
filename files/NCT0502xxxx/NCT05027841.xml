  </study_design_info>
  <primary_outcome>
    <measure>Sensorial Assessment-1</measure>
    <time_frame>2 minutes</time_frame>
    <description>Semmes-Weinstein Monoflament</description>
  </primary_outcome>
  <primary_outcome>
    <measure>Sensorial Assessment-2</measure>
    <time_frame>2 minutes</time_frame>
    <description>Position test for lower extremity</description>
  </primary_outcome>
  <primary_outcome>
    <measure>Sensorial Assessment-3</measure>
    <time_frame>2 minutes</time_frame>
    <description>Kinestesia (Electrogoniometer)</description>
  </primary_outcome>
  <primary_outcome>
    <measure>Balance assessment</measure>
    <time_frame>30 second</time_frame>
    <description>Static balance via SportKAT-550</description>
  </primary_outcome>
  <primary_outcome>
    <measure>Gait parameters</measure>
    <time_frame>1 minute</time_frame>
    <description>BTS G-Walk Spatiotemporal gait parameters</description>
  </primary_outcome>
  <enrollment type="Anticipated">28</enrollment>
