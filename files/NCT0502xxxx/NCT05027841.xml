  <brief_title>Proprioception on Balance and Gait in Hemiparetics</brief_title>
  <official_title>Investigation of The Effect of Proprioception on Balance and Gait Functions in Hemiparetic Individuals</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Istanbul Aydın University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
