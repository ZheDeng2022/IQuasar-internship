  <brief_title>Computer Vision Syndrome Prevalence Among University Students</brief_title>
  <official_title>Prevalence and Severity of Computer Vision Syndrome Among Medical Students</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Sohag University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
