  </intervention>
  <intervention>
    <intervention_type>Drug</intervention_type>
    <intervention_name>Rituximab</intervention_name>
    <description>Rituximab IV 375 mg/m2 for 6 cycles</description>
    <arm_group_label>Rituximab plus Zandelisib</arm_group_label>
    <arm_group_label>Rituximab plus chemotherapy</arm_group_label>
    <other_name>Rituxan</other_name>
