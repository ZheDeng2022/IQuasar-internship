  <brief_title>Virtual Reality Compared to Nitrous Oxide for Labor Analgesia</brief_title>
  <official_title>Virtual Reality Compared to Nitrous Oxide for Labor Analgesia</official_title>
  <sponsors>
    <lead_sponsor>
