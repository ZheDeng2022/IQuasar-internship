  </study_design_info>
  <primary_outcome>
    <measure>Behavioral side effects</measure>
    <time_frame>3 weeks</time_frame>
    <description>Behavioral side effects are measured by SCL-90-R questionnaire</description>
  </primary_outcome>
  <number_of_arms>2</number_of_arms>
  <enrollment type="Anticipated">40</enrollment>
