  <brief_title>Clinical and Radiological Outcomes of Medacta Shoulder System (BE)</brief_title>
  <official_title>Clinical and Radiological Outcomes of Medacta Shoulder System (BE)</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Medacta International SA</agency>
      <agency_class>Industry</agency_class>
