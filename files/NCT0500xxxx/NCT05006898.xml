  <brief_title>Compound to Control Presbyopia Symptoms</brief_title>
  <official_title>Synergistic Use of Pilocarpine-Brimonidine-Oxymetazoline to Control Presbyopia Symptoms</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Optall Vision</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
  <source>Optall Vision</source>
  <oversight_info>
    <has_dmc>Yes</has_dmc>
