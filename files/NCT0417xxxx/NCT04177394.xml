  <brief_title>MitraClip EXPAND G4 Study</brief_title>
  <official_title>A Post-Market Study Assessment of the Safety and Performance of the MitraClip G4 System</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Abbott Medical Devices</agency>
      <agency_class>Industry</agency_class>
