    <time_frame>Baseline to Week 8</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Incidence of treatment-emergent adverse events (TEAEs) related to Belotero Balance</measure>
    <time_frame>From baseline to week 72-76</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Incidence of treatment-emergent serious adverse events (TESAEs) related to Belotero Balance</measure>
    <time_frame>From baseline to week 72-76</time_frame>
  </secondary_outcome>
