  <primary_outcome>
    <measure>Proportion of responders at Week 8 on the Merz Infraorbital Hollow Assessment Scale (MIHAS)</measure>
    <time_frame>Week 8</time_frame>
    <description>The MIHAS is a 5-grade scale.</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Global Aesthetic Improvement Scale (GAIS) score at Week 8, as assessed by the treating investigator</measure>
    <time_frame>Week 8</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Global Aesthetic Improvement Scale (GAIS) score at Week 8, as assessed by the subject</measure>
    <time_frame>Week 8</time_frame>
  </secondary_outcome>
  <secondary_outcome>
