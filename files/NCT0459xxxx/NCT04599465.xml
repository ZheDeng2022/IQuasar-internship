    <allocation>N/A</allocation>
    <intervention_model>Single Group Assignment</intervention_model>
    <primary_purpose>Treatment</primary_purpose>
    <masking>None (Open Label)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Change From Baseline in 2-hour Blood Glucose Levels Following an Oral Glucose Tolerance Test (OGTT) to the Average of Week 36 and Week 48</measure>
    <time_frame>Day 1 up to Week 48</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Proportion of Participants With Improvement in Dysglycemia Categorization at Week 48</measure>
    <time_frame>Day 1 up to Week 48</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Safety and Tolerability as Assessed by Number of Participants With Adverse Events (AEs) and Serious Adverse Events (SAEs)</measure>
    <time_frame>Day 1 up to Week 52</time_frame>
  </secondary_outcome>
