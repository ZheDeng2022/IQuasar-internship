  <brief_title>Open-label Extension Study of Brazikumab in Crohn's Disease</brief_title>
  <acronym>INTREPID OLE</acronym>
  <official_title>An Open-label, Long-term Extension Study of Brazikumab in Participants With Moderately to Severely Active Crohn's Disease (INTREPID OLE)</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>AstraZeneca</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
