  <brief_title>Personalized Treatment of Urogenital Cancers Depends on the Microbiome</brief_title>
  <official_title>Personalized Treatment of Urogenital Cancers Depends on the Microbiome</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Zealand University Hospital</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
