  <primary_outcome>
    <measure>Number of Participants (or Chart Reviews) With Complete Success of Primary Vitrectomy Surgery</measure>
    <time_frame>6 months</time_frame>
    <description>Complete success of primary vitrectomy surgery, defined as retinal re-attachment without the need for any additional surgical procedures.</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Number of Participants (or Chart Reviews) With Presence of Proliferative Vitreoretinopathy</measure>
    <time_frame>6 months</time_frame>
  </secondary_outcome>
  <number_of_arms>2</number_of_arms>
  <enrollment type="Actual">101</enrollment>
  <condition>Retinal Detachment</condition>
