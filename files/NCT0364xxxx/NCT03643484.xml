    <observational_model>Cohort</observational_model>
    <time_perspective>Prospective</time_perspective>
  </study_design_info>
  <target_duration>12 Months</target_duration>
  <primary_outcome>
    <measure>Incidence of Interstitial lung disease (including radiation pneumonitis) by post-chemoradiation therapy radiation pneumonitis (yes/no) and by severity (CTCAE grade)</measure>
    <time_frame>12 months</time_frame>
  </primary_outcome>
  <enrollment type="Actual">634</enrollment>
  <condition>Carcinoma, Non-Small-Cell Lung</condition>
  <eligibility>
    <study_pop>
      <textblock>
        Patients with locally advanced unresectable non-small cell lung cancer after definitive&#xD;
        chemoradiation therapy&#xD;
      </textblock>
    </study_pop>
    <sampling_method>Non-Probability Sample</sampling_method>
    <criteria>
      <textblock>
        Inclusion Criteria:&#xD;
&#xD;
        -Patients with locally advanced unresectable non-small cell lung cancer who are treated&#xD;
        with IMFINZI for the first time as maintenance therapy after definitive chemoradiation&#xD;
        therapy&#xD;
&#xD;
        Exclusion Criteria:&#xD;
&#xD;
        -No past history of hypersensitivity to the components of IMFINZI&#xD;
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>N/A</minimum_age>
    <maximum_age>N/A</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>
