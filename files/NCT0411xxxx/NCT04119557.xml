  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria:&#xD;
&#xD;
          -  Have a confirmed diagnosis of psoriasis for at least 6 months&#xD;
&#xD;
          -  Have active psoriasis plaques according to study- specific criteria&#xD;
&#xD;
          -  Be willing and able to undergo skin biopsies&#xD;
&#xD;
        Exclusion Criteria:&#xD;
&#xD;
          -  Have received certain topical medications for psoriasis within 14 days prior to&#xD;
             baseline&#xD;
&#xD;
          -  Have received certain systemic medications for psoriasis within 4 weeks prior to&#xD;
             baseline&#xD;
&#xD;
          -  Have received LY3471851 previously&#xD;
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>70 Years</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>
