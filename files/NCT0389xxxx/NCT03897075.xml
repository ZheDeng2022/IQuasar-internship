  <primary_outcome>
    <measure>The proportion of subjects with, at least, a score of &quot;0 - normal&quot; or &quot;1 - minimal nail psoriasis&quot; and a 2-point decrease from baseline as measured by the ViSENPsO.</measure>
    <time_frame>Week 28</time_frame>
    <description>Primary Efficacy Endpoint</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>The proportion of subjects who achieve at least a 75% improvement from baseline in total-mNAPSI</measure>
    <time_frame>Week 28</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>The proportion of subjects with at least 3 point decrease from baseline, in Nail Pain NRS score in subjects with baseline nail pain NRS score of &gt;3</measure>
    <time_frame>Week 28</time_frame>
  </secondary_outcome>
