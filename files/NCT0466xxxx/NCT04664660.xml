  <primary_outcome>
    <measure>Preterm delivery</measure>
    <time_frame>Time of delivery</time_frame>
    <description>Incidence of delivery before 37 weeks of pregnancy</description>
  </primary_outcome>
  <primary_outcome>
    <measure>Gestational diabetes</measure>
    <time_frame>after 18 weeks' pregnancy</time_frame>
    <description>fasting blood glucose &gt;17 mmol/L</description>
  </primary_outcome>
  <primary_outcome>
    <measure>Gestational hypertension</measure>
    <time_frame>after 20 weeks' gestation</time_frame>
    <description>blood pressure persistently &gt; 140/90 mm Hg</description>
  </primary_outcome>
  <primary_outcome>
    <measure>Assisted reproduction technologies</measure>
    <time_frame>before pregnancy</time_frame>
    <description>Patients who referred to assisted reproduction technologies</description>
  </primary_outcome>
  <primary_outcome>
    <measure>Cesarean Section</measure>
    <time_frame>Time of delivery</time_frame>
    <description>Robson Classifications</description>
  </primary_outcome>
  <primary_outcome>
    <measure>Postpartum hemorrhage</measure>
    <time_frame>Time of delivery</time_frame>
    <description>Greater than 500 mL estimated blood loss in a vaginal delivery or greater than 1000 mL estimated blood loss at the time of cesarean delivery</description>
  </primary_outcome>
  <primary_outcome>
    <measure>Birth-weight</measure>
    <time_frame>Time of delivery</time_frame>
    <description>small for gestational age (birth weight below the 10th percentile) or large for gestational age (birth weight over the 90th percentile)</description>
  </primary_outcome>
  <primary_outcome>
    <measure>Apgar scores</measure>
