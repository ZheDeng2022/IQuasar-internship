    <allocation>Randomized</allocation>
    <intervention_model>Parallel Assignment</intervention_model>
    <primary_purpose>Prevention</primary_purpose>
    <masking>Single (Participant)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>prevalence of postoperative ovarian adhesions</measure>
    <time_frame>3 months after operation</time_frame>
    <description>prevalence of postoperative ovarian adhesions</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>postoperative pain</measure>
    <time_frame>daily during postoperative hospitalisation and at follow up visit after 3 months</time_frame>
    <description>postoperative pain (NRS)</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>AMH</measure>
    <time_frame>at follow up visit after 3 months</time_frame>
    <description>AMH level</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>postoperative infectious complications</measure>
    <time_frame>up to 3 months after operation</time_frame>
    <description>postoperative infectious complications</description>
  </secondary_outcome>
  <number_of_arms>4</number_of_arms>
  <enrollment type="Actual">409</enrollment>
  <condition>Endometriosis</condition>
  <condition>Adhesion</condition>
  <condition>Surgery</condition>
  <arm_group>
    <arm_group_label>A</arm_group_label>
    <arm_group_type>Experimental</arm_group_type>
    <description>ovariopexy for 2 days</description>
  </arm_group>
  <arm_group>
    <arm_group_label>B</arm_group_label>
    <arm_group_type>Experimental</arm_group_type>
    <description>ovariopexy for 4 days</description>
  </arm_group>
  <arm_group>
