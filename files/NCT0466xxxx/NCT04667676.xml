  </study_design_info>
  <primary_outcome>
    <measure>Spirometer</measure>
    <time_frame>4 weeks</time_frame>
    <description>• Portable spirometer to measure forced vital capacity (FVC) and forced expiratory volume in 1 second (FEV1) from setting position.</description>
  </primary_outcome>
  <number_of_arms>2</number_of_arms>
  <enrollment type="Actual">40</enrollment>
  <condition>Inhalation Injury</condition>
  <arm_group>
