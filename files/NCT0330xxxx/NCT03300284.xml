  <brief_title>Decision Making and Psychological Outcomes in Low-Risk Papillary Thyroid Cancer</brief_title>
  <official_title>Decision Making and Psychological Outcomes in Low-Risk Papillary Thyroid Cancer</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Memorial Sloan Kettering Cancer Center</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
