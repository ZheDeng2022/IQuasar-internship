  <brief_title>MRI Findings of Encephalopathy Associated With Carbon Dioxide (CO2) Intoxication</brief_title>
  <sponsors>
    <lead_sponsor>
      <agency>China Medical University Hospital</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
