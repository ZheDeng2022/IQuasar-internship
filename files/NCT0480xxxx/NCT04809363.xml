  <brief_title>A Study of CDPATH™ to Help Manage and Treat Crohn's Disease</brief_title>
  <acronym>COMPASS-CD</acronym>
  <official_title>A Phase 4 Study Evaluating Real-World Use of CDPATH™, a Crohn's Disease Risk Prediction Tool</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Takeda</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
