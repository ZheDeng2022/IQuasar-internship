    <time_frame>11 months</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Assessment of PK Parameter: Clearance of ADG20</measure>
    <time_frame>11 months</time_frame>
  </secondary_outcome>
  <secondary_outcome>
