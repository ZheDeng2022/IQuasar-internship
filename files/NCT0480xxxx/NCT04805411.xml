  <secondary_outcome>
    <measure>Dermatology Life Quality Index (DLQI)</measure>
    <time_frame>Baseline to Week 24</time_frame>
    <description>Change from baseline in DLQI</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Safety parameters</measure>
    <time_frame>Baseline to Week 24</time_frame>
    <description>Incidence of AE, abnormal physical examinations, abnormal vital signs, abnormal ECG, and abnormal lab testing.</description>
  </secondary_outcome>
  <secondary_outcome>
