  </secondary_outcome>
  <secondary_outcome>
    <measure>Pharmacodynamics</measure>
    <time_frame>Baseline to Week 24</time_frame>
    <description>Total IgE level</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>immunogenicity</measure>
    <time_frame>Baseline to Week 24</time_frame>
    <description>Detection of anti-drug antibody (ADA)</description>
  </secondary_outcome>
  <number_of_arms>3</number_of_arms>
  <enrollment type="Anticipated">120</enrollment>
  <condition>Moderate-to-severe Atopic Dermatitis</condition>
  <arm_group>
    <arm_group_label>High-dose arm</arm_group_label>
    <arm_group_type>Experimental</arm_group_type>
    <description>600mg for 1st dose, and then 300 mg for 2-8nd doses, every 2 weeks, SC</description>
  </arm_group>
  <arm_group>
    <arm_group_label>low-dose arm</arm_group_label>
