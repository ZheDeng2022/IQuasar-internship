  </arm_group>
  <arm_group>
    <arm_group_label>placebo</arm_group_label>
    <arm_group_type>Placebo Comparator</arm_group_type>
    <description>placebo for 1-8 doses, every 2 weeks, SC</description>
  </arm_group>
  <intervention>
