  <brief_title>Study of the Use of Hearing Protection Equipment in the Occurence of Acute Acoustic Trauma in the Army</brief_title>
  <acronym>QPROTEC</acronym>
  <official_title>Study of the Use of Hearing Protection Equipment in the Occurence of Acute Acoustic Trauma in the Army</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Direction Centrale du Service de Santé des Armées</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
