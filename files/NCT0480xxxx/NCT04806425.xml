  <condition>Anesthesia</condition>
  <arm_group>
    <arm_group_label>IL group</arm_group_label>
    <arm_group_type>Active Comparator</arm_group_type>
    <description>receive 1.5 ml /kg intralipid 20% through Central venous line after sternotomy over 1 hour</description>
  </arm_group>
  <arm_group>
    <arm_group_label>NS group</arm_group_label>
    <arm_group_type>Placebo Comparator</arm_group_type>
    <description>recieve 1.5 ml /kg normal saline 0.9% through central venous line after sternotomy over 1 hour</description>
  </arm_group>
  <intervention>
    <intervention_type>Drug</intervention_type>
    <intervention_name>Intralipid</intervention_name>
    <description>intralipid is given as a preconditioning drug during off bump coronary artery bypass grafting surgery to measure efficacy in decreasing post operative ischemia</description>
    <arm_group_label>IL group</arm_group_label>
  </intervention>
  <intervention>
    <intervention_type>Drug</intervention_type>
    <intervention_name>Placebo</intervention_name>
    <description>normal saline is given in same volume to normal saline group over the same duration</description>
    <arm_group_label>NS group</arm_group_label>
  </intervention>
