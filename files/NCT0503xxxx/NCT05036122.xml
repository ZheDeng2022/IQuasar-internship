  <brief_title>Estimating Blood [Lactate] Non-Invasively</brief_title>
  <official_title>Validation of a Non-Invasive Instrument to Estimate Blood Lactate</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Mayo Clinic</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
