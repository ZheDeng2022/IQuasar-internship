  <eligibility>
    <study_pop>
      <textblock>
        Patients admitted to a rehabilitation clinic for patients ≥ 65 years in Stockholm County,&#xD;
        Sweden.&#xD;
      </textblock>
    </study_pop>
    <sampling_method>Probability Sample</sampling_method>
    <criteria>
      <textblock>
        Inclusion Criteria:&#xD;
&#xD;
          -  Admission to a rehabilitation clinic for patients ≥ 65 years in Stockholm County,&#xD;
             Sweden&#xD;
&#xD;
          -  Ability to understand oral and written instructions in Swedish&#xD;
&#xD;
          -  Ability to produce a voided urine-sample&#xD;
&#xD;
        Exclusion Criteria:&#xD;
&#xD;
          -  Severe cognitive failure and/or disability to understand oral and written instructions&#xD;
             in Swedish according to the healthcare-personnel's judgment at the ward&#xD;
&#xD;
          -  End-of-life care&#xD;
&#xD;
          -  Physical disability to produce voided urine-samples&#xD;
&#xD;
          -  Ongoing antimicrobial treatment for symptomatic bacteriuria&#xD;
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>65 Years</minimum_age>
    <maximum_age>N/A</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>
