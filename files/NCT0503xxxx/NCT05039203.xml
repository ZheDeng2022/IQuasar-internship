  </study_design_info>
  <primary_outcome>
    <measure>Symptomatic bacteriuria (urinary tract infection)</measure>
    <time_frame>Four weeks</time_frame>
    <description>Clinical diagnosis of urinary tract infection by a physician</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Asymptomatic bacteriuria (ABU)</measure>
    <time_frame>One day</time_frame>
    <description>Prevalence of ABU at the point of removal of an IUC compared with patients without IUC</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Persistence of ABU</measure>
    <time_frame>Four weeks</time_frame>
    <description>For how long does ABU persist after IUC-removal</description>
  </secondary_outcome>
  <number_of_groups>2</number_of_groups>
  <enrollment type="Anticipated">800</enrollment>
