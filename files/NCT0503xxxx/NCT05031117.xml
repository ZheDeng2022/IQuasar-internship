    <primary_purpose>Supportive Care</primary_purpose>
    <masking>Double (Participant, Investigator)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>incidence of bilateral sacral blockade</measure>
    <time_frame>Up to 30 minutes after local anesthetic injection</time_frame>
  </primary_outcome>
  <number_of_arms>2</number_of_arms>
  <enrollment type="Anticipated">50</enrollment>
  <condition>Gynecologic Disease, Vaginal Hysterectomy, Laparoscopic Surgeries</condition>
  <arm_group>
    <arm_group_label>Conventional Epidural Technique</arm_group_label>
    <arm_group_type>Experimental</arm_group_type>
  </arm_group>
  <arm_group>
    <arm_group_label>Dural Puncture Epidural technique using pencil-point 25G Whitacre needle</arm_group_label>
