  <brief_title>Medical Expulsive Therapy Post-SWL For Renal Stones</brief_title>
  <official_title>Medical Expulsive Therapy Post-SWL For Renal Stones. A Randomized Trial</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Mansoura University</agency>
      <agency_class>Other</agency_class>
