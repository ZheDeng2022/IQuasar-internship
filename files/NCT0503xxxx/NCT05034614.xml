  <brief_title>Pap Smear Collection With the Papette Brush</brief_title>
  <official_title>Pap Smear Collection With the Papette Brush: A Pragmatic Study</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Mayo Clinic</agency>
      <agency_class>Other</agency_class>
