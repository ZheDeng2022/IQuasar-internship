  <brief_title>Treatments Against RA and Effect on FDG-PET/CT</brief_title>
  <acronym>TARGET</acronym>
  <official_title>Treatments Against RA and Effect on FDG-PET/CT (The TARGET Trial)</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Brigham and Women's Hospital</agency>
      <agency_class>Other</agency_class>
