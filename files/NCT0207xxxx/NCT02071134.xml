    <arm_group_label>Parkinson's disease</arm_group_label>
    <description>Subjects with Parkinson's disease who will receive Vercise DBS for deep brain stimulation.</description>
  </arm_group>
  <intervention>
    <intervention_type>Device</intervention_type>
    <intervention_name>Deep Brain Stimulation (DBS)</intervention_name>
    <description>Subjects receiving Deep Brain Stimulation (DBS) for the treatment of their Parkinson's disease symptoms will be offered participation in this registry.</description>
    <arm_group_label>Parkinson's disease</arm_group_label>
    <other_name>Vercise DBS System</other_name>
    <other_name>Boston Scientific</other_name>
  </intervention>
  <eligibility>
    <study_pop>
      <textblock>
        Patients with symptoms of Parkinson's disease that are not adequately controlled with&#xD;
        medication&#xD;
      </textblock>
    </study_pop>
    <sampling_method>Non-Probability Sample</sampling_method>
    <criteria>
      <textblock>
        Key Inclusion Criteria:&#xD;
&#xD;
          -  Meets criteria established in locally applicable Vercise System Direction for Use&#xD;
&#xD;
          -  At least 18 years old&#xD;
&#xD;
        Key Exclusion Criteria:&#xD;
&#xD;
          -  Meets any contraindication in the Vercise System locally applicable Directions for Use&#xD;
&#xD;
        Subjects with significant cognitive or psychiatric impairment may be excluded in the&#xD;
        evaluation of GXT.&#xD;
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>N/A</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>
