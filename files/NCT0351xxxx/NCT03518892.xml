  <brief_title>Assessing Dietary Requirements in Relation to Resting Metabolic Rate in Spinal Cord Injury</brief_title>
  <official_title>Assessing Dietary Requirements in Relation to Resting Metabolic Rate in Spinal Cord Injury</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Milton S. Hershey Medical Center</agency>
      <agency_class>Other</agency_class>
