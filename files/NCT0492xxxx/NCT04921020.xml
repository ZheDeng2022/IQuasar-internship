  <brief_title>Assessment of Eyelid Topology and Kinetics Based on Deep Learning Method</brief_title>
  <official_title>Department of Ophthalmology</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Second Affiliated Hospital, School of Medicine, Zhejiang University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
