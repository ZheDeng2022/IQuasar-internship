    <intervention_name>GORE® VIABAHN® Endoprosthesis</intervention_name>
    <description>On Day 1, Participants will receive the GORE® VIABAHN® Endoprosthesis</description>
    <arm_group_label>GORE® VIABAHN® Endoprosthesis</arm_group_label>
  </intervention>
