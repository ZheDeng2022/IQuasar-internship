  <brief_title>Synuclein-One Study</brief_title>
  <official_title>Cutaneous Phosphorlyated α-synuclein Detection as a Biomarker of Synucleinopathy</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>CND Life Sciences</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
