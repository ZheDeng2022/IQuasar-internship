  <brief_title>Effects of Nightshift Work and Sleep Disturbances on Erectile Function</brief_title>
  <official_title>Effects of Nightshift Work and Sleep Disturbances on Erectile Function, Serum Testosterone Levels, Nocturia</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Istanbul Medipol University Hospital</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
    <collaborator>
      <agency>Prof. Dr. Cemil Tascıoglu Education and Research Hospital Organization</agency>
      <agency_class>Other</agency_class>
    </collaborator>
  </sponsors>
