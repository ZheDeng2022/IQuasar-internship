  </study_design_info>
  <target_duration>6 Months</target_duration>
  <primary_outcome>
    <measure>Erectile dysfunction</measure>
    <time_frame>3-6 months</time_frame>
    <description>Decreased scores of IIEF-5</description>
  </primary_outcome>
