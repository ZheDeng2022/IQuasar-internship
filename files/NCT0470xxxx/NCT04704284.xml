  <brief_title>Comparing MRI to CT on Pediatric Craniosynostosis.</brief_title>
  <official_title>Comparative Analysis of Zero Echo-time MRI and CT for Pediatric Craniosynostosis</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Mayo Clinic</agency>
      <agency_class>Other</agency_class>
