  <brief_title>A Study of the Effects of an Employee Wellness Program</brief_title>
  <sponsors>
    <lead_sponsor>
      <agency>Richard Deichmann, MD</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
