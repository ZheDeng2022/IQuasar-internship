    <time_frame>1 week</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>objective response rate</measure>
    <time_frame>up to 2 months</time_frame>
  </secondary_outcome>
  <number_of_arms>2</number_of_arms>
  <enrollment type="Anticipated">164</enrollment>
  <condition>Hepatocellular Carcinoma Stage III</condition>
  <arm_group>
    <arm_group_label>Neoadjuvant group</arm_group_label>
