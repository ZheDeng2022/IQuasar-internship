    <masking>Single (Investigator)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>1-year disease free survival</measure>
    <time_frame>1 year</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>1-year overall survival</measure>
    <time_frame>1 year</time_frame>
  </secondary_outcome>
  <secondary_outcome>
