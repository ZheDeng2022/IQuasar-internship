  <brief_title>Trevo® Retriever Registry (China)</brief_title>
  <official_title>Trevo® Retriever Registry (China)</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Stryker Neurovascular</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
