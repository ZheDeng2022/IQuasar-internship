  <primary_outcome>
    <measure>Fasting gastric volume as measured by single photon emission computed tomography (SPECT)</measure>
    <time_frame>9 Days</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Accommodation volume as measured by SPECT</measure>
    <time_frame>9 Days</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Satiation expressed as volume to fullness as measured by satiation test</measure>
    <time_frame>9 Days</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Gastric emptying half-time of solids as measured by scintigraphy</measure>
    <time_frame>9 Days</time_frame>
  </primary_outcome>
