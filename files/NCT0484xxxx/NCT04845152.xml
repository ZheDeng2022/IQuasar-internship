  <brief_title>Migraine in Electrohypersensitive Patients</brief_title>
  <official_title>Prevalence of Migraine Disease in Electrohypersensitive Patients</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>University Hospital, Montpellier</agency>
