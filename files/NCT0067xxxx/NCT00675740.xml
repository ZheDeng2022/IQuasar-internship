    <other_name>Avandia (GlaxoSmithKline)</other_name>
  </intervention>
  <intervention>
    <intervention_type>Behavioral</intervention_type>
    <intervention_name>physical exercise</intervention_name>
    <description>stationary bike 5-6 times a week</description>
    <arm_group_label>2</arm_group_label>
  </intervention>
