  <brief_title>Krill Oil and Muscle Weakness in Type 2 Diabetes</brief_title>
  <acronym>KRIMD</acronym>
  <official_title>Effects of Krill Oil Supplementation on Muscle Mass and Function in People With Muscle Weakness and Type 2 Diabetes</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Dasman Diabetes Institute</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
  <source>Dasman Diabetes Institute</source>
  <oversight_info>
    <has_dmc>No</has_dmc>
