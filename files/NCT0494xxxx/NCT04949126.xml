  <brief_title>Self Medication in Odontology (AUDE)</brief_title>
  <acronym>AUDE</acronym>
  <official_title>Self Medication and Pulpal and Periapical Dental Pain</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>University Hospital, Clermont-Ferrand</agency>
      <agency_class>Other</agency_class>
