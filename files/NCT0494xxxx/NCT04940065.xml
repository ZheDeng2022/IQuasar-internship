  <brief_title>Special Drug-use Surveillance for Kesimpta for s.c. Injection 20 mg Pen</brief_title>
  <official_title>Special Drug-use Surveillance for Kesimpta for s.c. Injection 20 mg Pen (Relapsing-remitting Multiple Sclerosis and Active Secondary Progressive Multiple Sclerosis)</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Novartis Pharmaceuticals</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
