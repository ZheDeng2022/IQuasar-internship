  <primary_outcome>
    <measure>Change from baseline seizures frequency 1</measure>
    <time_frame>12 months</time_frame>
    <description>change from baseline seizures frequency at 12 months, seizure frequency is defined as the number of seizure attacks during 3 months before baseline or endpoint)</description>
  </primary_outcome>
  <primary_outcome>
    <measure>Change from baseline seizures frequency 2</measure>
    <time_frame>24 months</time_frame>
    <description>change from baseline seizures frequency at 24 months, seizure frequency is defined as the number of seizure attacks during 3 months before baseline or endpoint)</description>
  </primary_outcome>
