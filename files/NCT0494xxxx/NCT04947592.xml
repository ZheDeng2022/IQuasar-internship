  <primary_outcome>
    <measure>Best corrected visual acuity (BCVA)</measure>
    <time_frame>6 months</time_frame>
    <description>Change in BCVA in LOG MARS(logarithm minimum angle of resolution</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>optical coherence tomography (OCT) foveal thickness</measure>
    <time_frame>6 months</time_frame>
    <description>change in foveal thickness measured in micrometer</description>
  </secondary_outcome>
  <enrollment type="Anticipated">10</enrollment>
  <condition>Fungal Retinitis</condition>
  <intervention>
