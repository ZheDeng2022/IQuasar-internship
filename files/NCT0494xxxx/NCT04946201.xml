  <primary_outcome>
    <measure>Risk of Obstructive Sleep Apnea</measure>
    <time_frame>30 months</time_frame>
    <description>Percentage of participants who develop obstructive sleep apnea following orthodontic treatment.</description>
  </primary_outcome>
