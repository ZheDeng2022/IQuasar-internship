  <brief_title>The Impact of Sinopharm COVID-19 Vaccination on Male Fertility</brief_title>
  <official_title>The Impact of Sinopharm COVID-19 Vaccination on Male Fertility of the Egyptian Population</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Cairo University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
