  <primary_outcome>
    <measure>Distance VA (logMAR) with study lenses</measure>
    <time_frame>Day 8 (-0/+3 days), each product</time_frame>
    <description>Distance VA with study lenses will be collected in logarithm Minimum Angle of Resolution (logMAR) for each eye.</description>
  </primary_outcome>
  <number_of_arms>2</number_of_arms>
  <enrollment type="Actual">60</enrollment>
  <condition>Myopia</condition>
  <condition>Ametropia</condition>
  <arm_group>
    <arm_group_label>Precision1, then Infuse</arm_group_label>
    <arm_group_type>Other</arm_group_type>
    <description>Verofilcon A contact lenses worn first, with kalifilcon A contact lenses worn second, as randomized. Each product will be worn bilaterally (in both eyes) for 8 -0/+3 days in a daily disposable modality.</description>
  </arm_group>
  <arm_group>
    <arm_group_label>Infuse, then Precision1</arm_group_label>
    <arm_group_type>Other</arm_group_type>
    <description>Kalifilcon A contact lenses worn first, with verofilcon A contact lenses worn second, as randomized. Each product will be worn bilaterally (in both eyes) for 8 -0/+3 days in a daily disposable modality.</description>
  </arm_group>
  <intervention>
    <intervention_type>Device</intervention_type>
    <intervention_name>Verofilcon A contact lenses</intervention_name>
    <description>Commercially available silicone hydrogel contact lenses for daily disposable wear</description>
    <arm_group_label>Infuse, then Precision1</arm_group_label>
    <arm_group_label>Precision1, then Infuse</arm_group_label>
    <other_name>PRECISION1™</other_name>
  </intervention>
  <intervention>
