  <brief_title>CYPRESS Efficacy and Safety Study Extension</brief_title>
  <acronym>CYPRESS</acronym>
  <official_title>Control of Myopia Using Peripheral Diffusion Lenses: Efficacy and Safety Study Extension</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>SightGlass Vision, Inc.</agency>
