  <brief_title>A Study of CC-99677 in Participants With Active Ankylosing Spondylitis</brief_title>
  <acronym>AS SpA axSpA</acronym>
  <official_title>A Phase 2a Multicenter Trial of CC-99677 in Moderate-to-severe Ankylosing Spondylitis</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Celgene</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
