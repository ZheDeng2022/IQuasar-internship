  <brief_title>Radiographic Imaging Validation and EvALuation for Angio iFR (ReVEAL iFR)</brief_title>
  <acronym>ReVEAL</acronym>
  <official_title>Radiographic Imaging Validation and EvALuation for Angio iFR (ReVEAL iFR)</official_title>
  <sponsors>
