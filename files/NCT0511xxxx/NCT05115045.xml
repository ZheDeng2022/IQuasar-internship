  <brief_title>Plasma SARS-CoV-2 RNA Levels in Critically Ill COVID-19 Patients</brief_title>
  <acronym>pRNA-COVID-ICU</acronym>
  <official_title>Plasma SARS-CoV-2 RNA Levels in Critically Ill COVID-19 Patients (pRNA-COVID-ICU)</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Hopital of Melun</agency>
      <agency_class>Other</agency_class>
