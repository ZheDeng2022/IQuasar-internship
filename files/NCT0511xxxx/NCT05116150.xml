  <primary_outcome>
    <measure>Systolic blood pressure (mmHg)</measure>
    <time_frame>7 days</time_frame>
    <description>by sphygmomanometer</description>
  </primary_outcome>
  <primary_outcome>
    <measure>Diastolic blood pressure (mmHg)</measure>
