  </primary_outcome>
  <primary_outcome>
    <measure>Mean arterial pressure (mmHg)</measure>
    <time_frame>7 days</time_frame>
    <description>calculated (MAP = (SBP-DBP)/3 + DBP</description>
  </primary_outcome>
  <primary_outcome>
    <measure>Heart rate (bpm)</measure>
    <time_frame>7 days</time_frame>
    <description>measured using Physio Flow (non-invasive device)</description>
  </primary_outcome>
  <primary_outcome>
    <measure>Stroke volume (ml)</measure>
    <time_frame>7 days</time_frame>
    <description>measured using Physio Flow (non-invasive device)</description>
  </primary_outcome>
  <primary_outcome>
    <measure>Cardiac output (l/min)</measure>
    <time_frame>7 days</time_frame>
    <description>calculated: HR (bpm) x SV (ml)</description>
  </primary_outcome>
  <primary_outcome>
    <measure>Total vascular conductance (ml/min/mmHg)</measure>
    <time_frame>7 days</time_frame>
    <description>calculated: CO (ml/min) / MAP (mmHg)</description>
  </primary_outcome>
