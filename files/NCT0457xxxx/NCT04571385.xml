  <arm_group>
    <arm_group_label>Part 2: AP30663</arm_group_label>
    <arm_group_type>Experimental</arm_group_type>
    <description>Participants will receive a single dose of one of the multiple dose levels of AP30663.</description>
  </arm_group>
  <arm_group>
    <arm_group_label>Part 2: Placebo</arm_group_label>
