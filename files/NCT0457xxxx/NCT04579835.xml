  <brief_title>Twin A Breech External Cephalic Version Intervention Trial (TWEXIT)</brief_title>
  <acronym>TWEXIT</acronym>
  <official_title>Twin A Breech External Cephalic Version Intervention Trial</official_title>
  <sponsors>
    <lead_sponsor>
