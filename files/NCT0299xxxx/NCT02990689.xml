    <primary_purpose>Treatment</primary_purpose>
    <masking>Double (Participant, Outcomes Assessor)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Intermediate visual acuity</measure>
    <time_frame>12 months</time_frame>
  </primary_outcome>
  <secondary_outcome>
