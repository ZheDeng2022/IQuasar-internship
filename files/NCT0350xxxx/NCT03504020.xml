  <brief_title>ECG Belt for CRT Response</brief_title>
  <official_title>ECG Belt for CRT Response</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Medtronic Cardiac Rhythm and Heart Failure</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
