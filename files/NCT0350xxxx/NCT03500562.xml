  <primary_outcome>
    <measure>Number of adverse events (AEs)</measure>
    <time_frame>Approximately 72 weeks</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Number of serious adverse events (SAEs)</measure>
    <time_frame>Approximately 72 weeks</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Number of participants who discontinue therapy due to an AE</measure>
    <time_frame>Approximately 72 weeks</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Effectiveness of daclatasvir DCV-based therapy</measure>
    <time_frame>Approximately 72 weeks</time_frame>
    <description>As measured by Sustained virologic response 12 weeks after the end of treatment (SVR12) Achieved SVR12: Hepatitis C virus (HCV) RNA below the lower limit of quantification (LLOQ)</description>
  </secondary_outcome>
