  <brief_title>Dose Optimization Study of Idelalisib in Follicular Lymphoma</brief_title>
  <official_title>Dose Optimization Study of Idelalisib in Follicular Lymphoma</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Gilead Sciences</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
