  <brief_title>Neuroblastoma Biology Study</brief_title>
  <official_title>N2004-05: Neuroblastoma Biology Study</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>New Approaches to Neuroblastoma Therapy Consortium</agency>
