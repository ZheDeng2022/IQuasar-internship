  <brief_title>TD01 Master Study (Safety and Efficacy Study)</brief_title>
  <official_title>TD01 Master Study (Safety and Efficacy Study)</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Biotronik SE &amp; Co. KG</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
