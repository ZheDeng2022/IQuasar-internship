  <brief_title>Evaluation of Sodium Fluorescein Use During Intraoperative Cystoscopy After Total Laparoscopic Hysterectomy</brief_title>
  <official_title>Evaluation of Sodium Fluorescein Use During Intraoperative Cystoscopy After Total Laparoscopic Hysterectomy</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>CHU de Quebec-Universite Laval</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
