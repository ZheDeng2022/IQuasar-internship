  <primary_outcome>
    <measure>Percent Change in Central Corneal Thickness</measure>
    <time_frame>8 hours</time_frame>
    <description>Percent change in central corneal thickness from evening to morning (after 8 hours with lid closed)</description>
  </primary_outcome>
