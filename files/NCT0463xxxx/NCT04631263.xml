    <intervention_name>Hyaluronic Acid</intervention_name>
    <description>Injectable gel. Chin correction and augmentation for retrusion</description>
    <arm_group_label>Treatment Group</arm_group_label>
    <other_name>GP0109</other_name>
  </intervention>
  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria:&#xD;
&#xD;
          -  subject willing to comply with the requirements of the study&#xD;
&#xD;
          -  subject intent to receive treatment for augmentation and correction of retrusion in&#xD;
             the chin region&#xD;
&#xD;
          -  subject with mild or moderate (grade 1 or 2) on the GCRS&#xD;
&#xD;
        Exclusion Criteria:&#xD;
&#xD;
          -  subjects presenting with know allergy to HA (hyaluronic acid) filler or amide local&#xD;
             anesthetics&#xD;
&#xD;
          -  subjects with any previous facial surgery below the nasal line or previous HA filler&#xD;
             or collagen below the nasal line within 12 months&#xD;
&#xD;
          -  subjects in any other interventional clinical study within 30 days before baseline&#xD;
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>19 Years</minimum_age>
    <maximum_age>N/A</maximum_age>
    <healthy_volunteers>Accepts Healthy Volunteers</healthy_volunteers>
