    <measure>50% Reduction in Proteinuria or Proteinuria &lt; 150mg/Day</measure>
    <time_frame>6 months</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>25% Improvement in the MDRD eGFR</measure>
    <time_frame>6 months</time_frame>
  </secondary_outcome>
