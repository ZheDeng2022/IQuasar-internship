  <brief_title>Extravascular ICD Pivotal Study</brief_title>
  <acronym>EV ICD</acronym>
  <official_title>Extravascular ICD Pivotal Study (EV ICD)</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Medtronic Cardiac Rhythm and Heart Failure</agency>
      <agency_class>Industry</agency_class>
