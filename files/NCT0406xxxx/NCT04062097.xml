  <brief_title>Registration of Idarucizumab for Patients With IntraCranial Hemorrhage</brief_title>
  <acronym>RIC-ICH</acronym>
  <official_title>Registration of Idarucizumab for Patients With IntraCranial Hemorrhage (RIC-ICH)</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>University Hospital, Essen</agency>
      <agency_class>Other</agency_class>
