  <brief_title>MEASuRE: Metreleptin Effectiveness And Safety Registry</brief_title>
  <acronym>MEASuRE</acronym>
  <official_title>MEASuRE: Metreleptin Effectiveness And Safety Registry</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Aegerion Pharmaceuticals, Inc.</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
