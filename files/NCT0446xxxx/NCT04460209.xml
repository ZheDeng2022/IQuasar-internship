  <brief_summary>
    <textblock>
      This study will provide ongoing, high quality data on the safety, performance, and clinical&#xD;
      benefits of Abbott's EP devices in a real-world setting.&#xD;
    </textblock>
  </brief_summary>
  <overall_status>Recruiting</overall_status>
  <start_date type="Actual">June 12, 2020</start_date>
  <completion_date type="Anticipated">December 2031</completion_date>
  <primary_completion_date type="Anticipated">November 2030</primary_completion_date>
  <study_type>Observational [Patient Registry]</study_type>
