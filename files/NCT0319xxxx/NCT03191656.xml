  <primary_outcome>
    <measure>Serious Adverse Events</measure>
    <time_frame>30 days</time_frame>
    <description>Device and or procedure related serious adverse cardiac events</description>
  </primary_outcome>
  <primary_outcome>
    <measure>New York Heart Classification</measure>
