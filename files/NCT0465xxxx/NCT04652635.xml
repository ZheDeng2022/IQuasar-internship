  <brief_title>Management of Nailbed Injuries</brief_title>
  <official_title>Management of Nailbed Injuries</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Stanford University</agency>
      <agency_class>Other</agency_class>
