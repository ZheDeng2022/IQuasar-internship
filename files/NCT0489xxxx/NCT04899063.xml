  <primary_outcome>
    <measure>Proportion of subjects who achieve a decrease in medication-free mean diurnal IOP (DIOP) from baseline of at least 20%</measure>
    <time_frame>24 Month</time_frame>
    <description>Proportion of subjects who achieve a decrease in medication-free mean diurnal IOP (DIOP) from baseline of at least 20%</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Mean Change in medication-free DIOP from baseline</measure>
    <time_frame>24 Month</time_frame>
    <description>Mean change in medication-free DIOP from baseline</description>
  </secondary_outcome>
  <enrollment type="Anticipated">318</enrollment>
