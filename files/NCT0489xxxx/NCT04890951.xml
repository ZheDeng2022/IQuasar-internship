  <primary_outcome>
    <measure>Anatomic failure to correct apical POP</measure>
    <time_frame>1 year</time_frame>
    <description>Descent of the apex (POP-Q point C) equal to or beyond one half of the total vaginal length</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Overall failure to correct POP</measure>
    <time_frame>1 year</time_frame>
    <description>POP-Q stage ≥2 in any compartment, or re-treatment of POP with physiotherapy, pessary, or repeat surgery</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Subjective failure to correct POP</measure>
    <time_frame>6 weeks; 1 year</time_frame>
    <description>Patient report of bulge symptoms at 6 weeks or 1 year, using one item on the PFDI-20: &quot;Do you usually have a bulge or something falling out that you can see or feel in the vaginal area?&quot;</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Length of surgery (minutes)</measure>
    <time_frame>Peri-operative</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Estimated blood loss during surgery &gt;500 mL</measure>
    <time_frame>Peri-operative</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Procedural complications</measure>
    <time_frame>Peri-operative</time_frame>
    <description>E.g., peri-operative blood transfusion, visceral injury</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Post-operative infection</measure>
