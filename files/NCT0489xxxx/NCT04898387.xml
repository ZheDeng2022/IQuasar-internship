  <primary_outcome>
    <measure>IOP assessment</measure>
    <time_frame>Week 12</time_frame>
    <description>The primary efficacy endpoint is the change from Baseline (Day 1) to Week 12 in IOP</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Ocular Sign</measure>
    <time_frame>Week 6 Week 12</time_frame>
    <description>Ocular sign will be assessed through slit lamp examination at each visits</description>
  </secondary_outcome>
