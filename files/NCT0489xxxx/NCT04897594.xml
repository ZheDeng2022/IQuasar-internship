  </secondary_outcome>
  <secondary_outcome>
    <measure>Plasma and urine concentration of TERN-201- Tmax</measure>
    <time_frame>12 weeks and 6 weeks respectively</time_frame>
    <description>Time to reach maximum measured plasma concentration</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Plasma and urine concentration of TERN-201- T1/2</measure>
    <time_frame>12 weeks and 6 weeks respectively</time_frame>
    <description>determination of half-life</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Percent Change from baseline in plasma VAP-1/SSAO activity</measure>
    <time_frame>12 weeks</time_frame>
  </secondary_outcome>
