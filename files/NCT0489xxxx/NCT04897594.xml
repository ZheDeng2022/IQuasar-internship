    <masking_description>Triple</masking_description>
  </study_design_info>
  <primary_outcome>
    <measure>Subject incidence of adverse events for TERN-201 versus placebo</measure>
    <time_frame>12 weeks</time_frame>
  </primary_outcome>
  <secondary_outcome>
