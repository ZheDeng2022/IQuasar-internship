  <primary_outcome>
    <measure>The rate of sympathetic plexus injury</measure>
    <time_frame>Postoperative day one</time_frame>
    <description>Edema of the lower extremity, change of skin temperature, pain, or paresthesia without other medical conditions</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Other complications</measure>
    <time_frame>Postoperative 6 month</time_frame>
    <description>Injury of vascular structure, ureter, peritoneum, or bowel.</description>
  </secondary_outcome>
  <enrollment type="Anticipated">200</enrollment>
  <condition>Fusion of Spine, Lumbosacral Region</condition>
