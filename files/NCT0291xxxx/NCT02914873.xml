  <brief_title>SPCG17: Prostate Cancer Active Surveillance Trigger Trial</brief_title>
  <acronym>PCASTT</acronym>
  <official_title>SPCG17: Prostate Cancer Active Surveillance Trigger Trial (PCASTT)</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Uppsala University</agency>
