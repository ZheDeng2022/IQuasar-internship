  <brief_title>The Effect of Vitamin D Supplementation on the Pulmonary Functions of Adult Asthmatic Egyptian Patients</brief_title>
  <official_title>The Effect of Vitamin D Supplementation on the Pulmonary Functions of Adult Asthmatic Egyptian Patients</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Cairo University</agency>
      <agency_class>Other</agency_class>
