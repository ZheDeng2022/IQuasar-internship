  <primary_outcome>
    <measure>Implant survival rate</measure>
    <time_frame>Baseline; 10-year at the 20-year follow-up examination</time_frame>
    <description>Change in percentage of dental implants in situ / dental implants placed</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Marginal Bone loss</measure>
    <time_frame>Baseline; 10-year at the 20-year follow-up examination</time_frame>
    <description>Changes in mm of the peri-implant marginal bone loss</description>
  </secondary_outcome>
