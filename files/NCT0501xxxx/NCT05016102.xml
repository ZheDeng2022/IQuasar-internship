  </study_design_info>
  <primary_outcome>
    <measure>Behavioral performance</measure>
    <time_frame>around 10-30 minutes after exercise intervention</time_frame>
    <description>reaction time of stroop and simon task following congruent and incongruent condition.</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>P3 component</measure>
    <time_frame>around 10-30 minutes after exercise intervention</time_frame>
    <description>P3 of event-related potentials component, a larger P3 amplitude and shorter P3 latency means better performance</description>
  </secondary_outcome>
  <other_outcome>
    <measure>EEG</measure>
    <time_frame>around 10-30 minutes after exercise intervention</time_frame>
    <description>Alpha power/ event-related desynchronization (ERD)</description>
  </other_outcome>
  <number_of_arms>3</number_of_arms>
  <enrollment type="Actual">25</enrollment>
  <condition>Exercise</condition>
  <condition>ADHD</condition>
  <condition>Executive Dysfunction</condition>
  <arm_group>
    <arm_group_label>Control condition</arm_group_label>
    <arm_group_type>Other</arm_group_type>
    <description>30 min to watching exercise-related video</description>
  </arm_group>
  <arm_group>
    <arm_group_label>Aerobic exercise condition</arm_group_label>
    <arm_group_type>Other</arm_group_type>
    <description>20 minutes of moderate-intensity exercise on treadmill</description>
  </arm_group>
  <arm_group>
    <arm_group_label>Resistance exercise condition</arm_group_label>
    <arm_group_type>Other</arm_group_type>
    <description>One set of 15 repetition 9 muscle exercises dumbbell squat with chair, dumbbell right/left foot lunge, sit-up, push-up, back muscle with superman, dumbbell right/left bicep curl, dumbbell calf raise.</description>
  </arm_group>
  <intervention>
    <intervention_type>Other</intervention_type>
