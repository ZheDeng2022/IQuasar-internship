      <agency>Lung Therapeutics, Inc</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
  <source>Lung Therapeutics, Inc</source>
  <oversight_info>
    <has_dmc>Yes</has_dmc>
