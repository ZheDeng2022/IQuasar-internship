  <primary_outcome>
    <measure>Reduction in targeted Ichthyosis severity</measure>
    <time_frame>12 weeks</time_frame>
    <description>Proportion of subjects with Visual Index for Ichthyosis Severity (VIIS) treatment success, defined as 50% or greater decreased in VIIS scaling score</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Reduction in overall Ichthyosis severity</measure>
    <time_frame>12 weeks</time_frame>
    <description>Investigator Global Assessment decrease in disease severity by at least 2 grades</description>
  </secondary_outcome>
