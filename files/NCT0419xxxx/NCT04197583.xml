  <brief_title>Double-J PLUS Postmarket Registry</brief_title>
  <official_title>Boston Scientific Double-J PLUS Ureteral Stent Postmarket Patient Registry</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Boston Scientific Corporation</agency>
      <agency_class>Industry</agency_class>
