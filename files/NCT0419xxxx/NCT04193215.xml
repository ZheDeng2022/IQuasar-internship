  <brief_title>V114 and Acute Otitis Media (V114-032/PNEU-ERA)</brief_title>
  <official_title>A Study of V114 and Acute Otitis Media in Children (PNEU-ERA)</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Merck Sharp &amp; Dohme Corp.</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
