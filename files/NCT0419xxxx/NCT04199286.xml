  <brief_title>Bilateral and Unilateral Horizontal Strabismus Surgery</brief_title>
  <official_title>Assessment of Symmetrical and Asymmetrical Horizontal Strabismus Surgery: Efficacy and Lateral Incomitance</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Minia University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
