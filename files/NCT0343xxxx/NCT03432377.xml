  <brief_title>Coffee Consumption and NASH in the French Population.</brief_title>
  <acronym>COCANASH</acronym>
  <official_title>Coffee Consumption and NASH in the French Population.</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Versailles Hospital</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
