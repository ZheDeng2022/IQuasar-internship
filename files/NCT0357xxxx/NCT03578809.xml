  <primary_outcome>
    <measure>Infarct size</measure>
    <time_frame>10-12 weeks</time_frame>
    <description>Infarct size as a percentage of LV mass measured on delayed-enhanced CMR imaging 10-12 weeks post-MI compared to placebo.</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Ejection Fraction</measure>
    <time_frame>10-12 weeks</time_frame>
    <description>EF measured by cine MRI at 10-12 weeks post-MI compared to placebo.</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Non Calcified Plaque Volume</measure>
    <time_frame>10-12 weeks</time_frame>
    <description>Change in NCPV in the coronary arteries from index CTA (Computed Tomography) to 10-12 weeks post-MI compared with placebo.</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Myocardial Mass</measure>
    <time_frame>10-12 weeks</time_frame>
    <description>Myocardial mass</description>
  </secondary_outcome>
