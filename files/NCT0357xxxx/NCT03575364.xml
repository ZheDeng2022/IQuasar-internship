  <brief_title>ClotTriever Outcomes (CLOUT) Registry</brief_title>
  <acronym>CLOUT</acronym>
  <official_title>ClotTriever Outcomes (CLOUT) Registry</official_title>
  <sponsors>
    <lead_sponsor>
