  <condition>Craniotomy</condition>
  <condition>Spinal Surgery</condition>
  <condition>Duraplasty</condition>
  <arm_group>
    <arm_group_label>Neuro-Patch</arm_group_label>
    <description>Patients receiving Neuro-Patch® for duraplasty</description>
  </arm_group>
  <eligibility>
    <study_pop>
      <textblock>
        adult patients&#xD;
      </textblock>
    </study_pop>
    <sampling_method>Non-Probability Sample</sampling_method>
    <criteria>
      <textblock>
        Inclusion Criteria:&#xD;
&#xD;
          -  Written informed consent&#xD;
&#xD;
          -  Age &gt; 18 years&#xD;
&#xD;
          -  Duraplasty using Neuro-Patch® according to the Instructions for Use&#xD;
&#xD;
        Exclusion Criteria:&#xD;
&#xD;
          -  Use in infected regions&#xD;
&#xD;
          -  Use in open cerebrocranial traumata&#xD;
&#xD;
          -  Use in open spina bifida&#xD;
&#xD;
          -  Known hypersensitivity to implant materials&#xD;
&#xD;
          -  Pregnancy&#xD;
&#xD;
          -  Representation by a legal guardian or under involuntary commitment&#xD;
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>N/A</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>
