    <masking>None (Open Label)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Improvement in urinary urgency incontinence (UUI) episodes based on patient voiding diary</measure>
    <time_frame>6 months</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Safety - incidence of adverse events</measure>
    <time_frame>12 months</time_frame>
  </primary_outcome>
  <number_of_arms>1</number_of_arms>
  <enrollment type="Actual">150</enrollment>
