  <primary_outcome>
    <measure>Glucose infusion rate</measure>
    <time_frame>3.5 hours</time_frame>
    <description>Glucose infusion rate required to maintain euglycemia during insulin clamp with and without endothelin blockade</description>
  </primary_outcome>
  <primary_outcome>
    <measure>Leg blood flow</measure>
    <time_frame>3.5 hours</time_frame>
    <description>Effect of endothelin receptor blockade on leg blood flow (ultrasound Doppler) during insulin clamp with and without endothelin blockade</description>
  </primary_outcome>
