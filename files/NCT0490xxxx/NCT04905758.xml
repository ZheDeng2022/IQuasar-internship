    <measure>marginal bone level</measure>
    <time_frame>12 months</time_frame>
    <description>measured in mm using X-ray</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>pink estheic score</measure>
    <time_frame>12 months</time_frame>
    <description>from 0-12. 0 means bad aesthetic of soft tissue. 12 means the ultimate aesthetics of soft tissue</description>
  </secondary_outcome>
