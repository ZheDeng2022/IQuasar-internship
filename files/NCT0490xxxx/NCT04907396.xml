  <eligibility>
    <study_pop>
      <textblock>
        Up to 100,000 participants who meet eligibility requirements&#xD;
      </textblock>
    </study_pop>
    <sampling_method>Non-Probability Sample</sampling_method>
    <criteria>
      <textblock>
        Inclusion Criteria:&#xD;
&#xD;
          -  The IWCR is open to anyone 18 years or older who is considering weight loss, is&#xD;
             actively engaged in weight loss, or has previously attempted weight loss (including&#xD;
             successful and unsuccessful attempts)&#xD;
&#xD;
        Exclusion Criteria:&#xD;
&#xD;
          1. Minors, below the age of 18&#xD;
&#xD;
          2. Adults who are not able to give informed consent online and complete online surveys&#xD;
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>N/A</maximum_age>
    <healthy_volunteers>Accepts Healthy Volunteers</healthy_volunteers>
  </eligibility>
