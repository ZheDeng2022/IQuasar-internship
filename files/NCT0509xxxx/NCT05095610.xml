    <time_frame>1 year</time_frame>
    <description>Time of use of Flash glucose monitoring (% possible time of use)</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>TIR</measure>
    <time_frame>1 year</time_frame>
    <description>Time in range (TIR, 3.9-10 mmol/L) of the interstitial glucose</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>TAR</measure>
    <time_frame>1 year</time_frame>
    <description>Time above range (TAR, &gt;10 mmol/L) of the interstitial glucose</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>TBR</measure>
    <time_frame>1 year</time_frame>
    <description>Time bellow range (TBR, &lt;3.9 mmol/L) of the interstitial glucose</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>%CV</measure>
    <time_frame>1 year</time_frame>
