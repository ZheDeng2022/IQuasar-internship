  <brief_summary>
    <textblock>
      To evaluate the wear experience of current monthly replacement contact lens wearers after&#xD;
      they have been fit into a daily disposable contact lens.&#xD;
    </textblock>
  </brief_summary>
  <overall_status>Recruiting</overall_status>
  <start_date type="Actual">May 28, 2021</start_date>
  <completion_date type="Anticipated">December 2021</completion_date>
  <primary_completion_date type="Anticipated">November 2021</primary_completion_date>
