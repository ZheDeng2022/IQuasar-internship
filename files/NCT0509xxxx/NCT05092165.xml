  <primary_outcome>
    <measure>Composite Endpoint</measure>
    <time_frame>6 hours</time_frame>
    <description>Composite endpoint of: Increase in vasopressor dose in at least 15% of initial dose or interruption of dialysis or reduction in ultrafiltration rate</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Hypotension</measure>
