    <intervention_name>Methylene Blue</intervention_name>
    <description>1 mg per kilogram bolus, followed by 0.1 milligram per kilogram during renal replacement therapy</description>
    <arm_group_label>Methylene Blue</arm_group_label>
  </intervention>
  <intervention>
    <intervention_type>Other</intervention_type>
