  <brief_title>Adult Tic Disorders Registry</brief_title>
  <acronym>RegisTICs</acronym>
  <official_title>Adult Tic Disorders Registry</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Assistance Publique - Hôpitaux de Paris</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
