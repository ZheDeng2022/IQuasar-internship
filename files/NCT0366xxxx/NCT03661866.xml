  <brief_title>A 5-year Longitudinal Observational Study of Patients Undergoing Therapy for IMISC</brief_title>
  <acronym>TARGET-DERM</acronym>
  <official_title>A 5-year Longitudinal Observational Study of Patients Undergoing Therapy for Immune-Mediated Inflammatory Skin Conditions</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Target PharmaSolutions, Inc.</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
