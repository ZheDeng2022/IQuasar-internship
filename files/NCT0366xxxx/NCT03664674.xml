  <brief_title>Phase 3 Study of OTO-104 in Subjects With Unilateral Meniere's Disease</brief_title>
  <official_title>A Prospective, Randomized, Double Blind, Placebo-controlled, Multicenter, Phase 3 Efficacy and Safety Study of OTO-104 Given as a Single Intratympanic Injection in Subjects With Unilateral Meniere's Disease</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Otonomy, Inc.</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
  <source>Otonomy, Inc.</source>
  <oversight_info>
    <has_dmc>No</has_dmc>
    <is_fda_regulated_drug>Yes</is_fda_regulated_drug>
    <is_fda_regulated_device>No</is_fda_regulated_device>
  </oversight_info>
  <brief_summary>
    <textblock>
      The purpose of this study is to evaluate the effectiveness of OTO-104 for the treatment of&#xD;
      Meniere's disease.&#xD;
    </textblock>
  </brief_summary>
  <overall_status>Completed</overall_status>
  <start_date type="Actual">August 27, 2018</start_date>
  <completion_date type="Actual">December 22, 2020</completion_date>
  <primary_completion_date type="Actual">December 22, 2020</primary_completion_date>
  <phase>Phase 3</phase>
  <study_type>Interventional</study_type>
  <has_expanded_access>No</has_expanded_access>
  <study_design_info>
    <allocation>Randomized</allocation>
    <intervention_model>Parallel Assignment</intervention_model>
    <intervention_model_description>Randomized, Double-blind, Placebo-controlled, Multicenter</intervention_model_description>
    <primary_purpose>Treatment</primary_purpose>
    <masking>Triple (Participant, Investigator, Outcomes Assessor)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>The number of definitive vertigo days</measure>
    <time_frame>3 months</time_frame>
    <description>subject diary</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Audiometry</measure>
    <time_frame>3 months</time_frame>
    <description>assessment of hearing function</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Otoscopic examination - use of an otoscope to assess the health of the ear</measure>
    <time_frame>3 months</time_frame>
    <description>assessment of the auditory canal and tympanic membrane</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Adverse events</measure>
    <time_frame>3 months</time_frame>
    <description>medical events reported by the subject</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Impact of vertigo experience on daily activities</measure>
    <time_frame>3 Months</time_frame>
    <description>subject diary</description>
  </secondary_outcome>
