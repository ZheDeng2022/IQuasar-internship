  <brief_title>Algorithm Using LINQ Sensors for Evaluation And Treatment of Heart Failure</brief_title>
  <acronym>ALLEVIATE-HF</acronym>
  <official_title>Algorithm Using LINQ Sensors for Evaluation And Treatment of Heart Failure</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Medtronic Cardiac Rhythm and Heart Failure</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
