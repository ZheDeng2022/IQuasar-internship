  <brief_title>Mucopolysaccharidosis (MPS) VI Clinical Surveillance Program (CSP)</brief_title>
  <official_title>MPS VI Clinical Surveillance Program (CSP)</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>BioMarin Pharmaceutical</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
