    <measure>Percentage Change From Baseline in Urine Protein/Creatinine Ratio (UPCR) as Measured in 24-hour Urine at Week 32</measure>
    <time_frame>Baseline, Week 32</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Percent Change from Baseline in 24-Hour Proteinuria at Week 32</measure>
    <time_frame>Baseline, Week 32</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Percentage of Participants with Partial Clinical Remission at Week 32</measure>
    <time_frame>Week 32</time_frame>
  </secondary_outcome>
  <secondary_outcome>
