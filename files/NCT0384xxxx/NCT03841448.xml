  <completion_date type="Anticipated">February 28, 2025</completion_date>
  <primary_completion_date type="Anticipated">February 28, 2022</primary_completion_date>
  <phase>Phase 2</phase>
  <study_type>Interventional</study_type>
  <has_expanded_access>No</has_expanded_access>
  <study_design_info>
    <allocation>Randomized</allocation>
    <intervention_model>Parallel Assignment</intervention_model>
    <primary_purpose>Treatment</primary_purpose>
    <masking>Triple (Participant, Care Provider, Investigator)</masking>
  </study_design_info>
