  <brief_title>Database Creation and Evaluation of Motor Function of Finger Tapping Measured by Magnetic Sensor System in Healthy Subjects</brief_title>
  <sponsors>
    <lead_sponsor>
      <agency>Hiroshima University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
