  <brief_title>Regional Anesthesia</brief_title>
  <official_title>Comparative Study Between Dural Puncture Epidural Technique and Conventional Epidural Technique During Elective Gynecological Vaginal Surgeries</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Reham Ali Abdelhaleem Abdelrahman</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
