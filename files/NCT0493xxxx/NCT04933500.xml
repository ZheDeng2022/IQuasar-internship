  <primary_outcome>
    <measure>Onset of bilateral sacral sensory loss</measure>
    <time_frame>4months</time_frame>
    <description>sacral sensory loss is observed by needle brick</description>
  </primary_outcome>
  <number_of_groups>2</number_of_groups>
  <enrollment type="Anticipated">88</enrollment>
  <condition>Anesthetic Regional Techniques for Vaginal Surgeries</condition>
  <arm_group>
    <arm_group_label>Conventional epidural</arm_group_label>
    <description>epidural needle: Tuhoy needle 1.3mm (18 G) with 20G multi-orifice epidural catheter</description>
  </arm_group>
  <arm_group>
    <arm_group_label>Dural puncture epidural</arm_group_label>
    <description>epidural needle;Tuhoy needle 1.3mm (18G)with 20G multi-orifice epidural catheter and 25G Whitacre spinal needle</description>
  </arm_group>
  <intervention>
    <intervention_type>Device</intervention_type>
    <intervention_name>Whitacre spinal needle</intervention_name>
    <description>25G Whitacre spinal needle</description>
    <arm_group_label>Dural puncture epidural</arm_group_label>
  </intervention>
