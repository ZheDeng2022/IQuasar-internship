  <primary_outcome>
    <measure>Ability to detect tonicity or volume change</measure>
    <time_frame>1 week</time_frame>
    <description>Accurately depict each of the four interventions with a sensitivity better than 0.9.</description>
  </primary_outcome>
  <number_of_groups>1</number_of_groups>
