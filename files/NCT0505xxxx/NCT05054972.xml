  <study_design_info>
    <observational_model>Cohort</observational_model>
    <time_perspective>Retrospective</time_perspective>
  </study_design_info>
  <primary_outcome>
    <measure>Renal function after surgery.</measure>
    <time_frame>24 months</time_frame>
    <description>The level of creatinine in the blood was measured after surgery at several time points.</description>
  </primary_outcome>
  <number_of_groups>4</number_of_groups>
  <enrollment type="Actual">110</enrollment>
  <condition>Aortic Aneurysm</condition>
