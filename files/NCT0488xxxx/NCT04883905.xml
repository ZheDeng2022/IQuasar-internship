  <brief_title>ELEVATE, a Registry of Patients With Acute Hepatic Porphyria (AHP)</brief_title>
  <acronym>ELEVATE</acronym>
  <official_title>ELEVATE, a Global Observational Longitudinal Prospective Registry of Patients With Acute Hepatic Porphyria (AHP)</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Alnylam Pharmaceuticals</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
