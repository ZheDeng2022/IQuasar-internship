  <brief_title>A Study of ADR-001 in Patients With Severe Pneumonia Caused by SARS-CoV-2 Infection (COVID-19)</brief_title>
  <official_title>An Phase2 Study of ADR-001 in Patients With Severe Pneumonia Caused by SARS-CoV-2 Infection</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Rohto Pharmaceutical Co., Ltd.</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
