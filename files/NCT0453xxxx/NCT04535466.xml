  <study_design_info>
    <observational_model>Cohort</observational_model>
    <time_perspective>Prospective</time_perspective>
  </study_design_info>
  <target_duration>2 Years</target_duration>
  <primary_outcome>
    <measure>the predictive model's area under curve(AUC)</measure>
    <time_frame>follow up for 2 year for individuals</time_frame>
    <description>To assess the predictive accuracy of the radiomic-based and clinical-based model by using operating characteristic curve (ROC) analysis and calculating the area under the curves (AUC).</description>
  </primary_outcome>
  <enrollment type="Anticipated">1000</enrollment>
  <condition>Breast Diseases</condition>
  <condition>Artificial Intelligence</condition>
  <condition>Dense Breast Density</condition>
  <condition>Predictive Cancer Model</condition>
  <eligibility>
    <study_pop>
      <textblock>
        Female patients aged 18-80 were included in our study who were diagnosed and treated at Sun&#xD;
        Yat-sen memorial hospital .&#xD;
      </textblock>
    </study_pop>
    <sampling_method>Non-Probability Sample</sampling_method>
    <criteria>
      <textblock>
        Inclusion Criteria:&#xD;
&#xD;
          -  Female patients with dense breast density tissue&#xD;
&#xD;
          -  Digital breast tomosynthesis was performed&#xD;
&#xD;
          -  Breast ultrasound examination was performed&#xD;
&#xD;
          -  Pathologic examination of breast lesions was performed&#xD;
&#xD;
        Exclusion Criteria:&#xD;
&#xD;
          -  Incomplete clinicopathologic data&#xD;
&#xD;
          -  Female patients with fatty glands of the breast&#xD;
      </textblock>
    </criteria>
    <gender>Female</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>80 Years</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>
