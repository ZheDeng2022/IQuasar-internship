  <primary_outcome>
    <measure>Change in modified International Index of Erectile Dysfunction score</measure>
    <time_frame>baseline and up to 1 month post-baseline</time_frame>
    <description>score 0 to 30 with higher score meaning overall better erectile function</description>
  </primary_outcome>
