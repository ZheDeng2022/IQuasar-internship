  <primary_outcome>
    <measure>accuracy</measure>
    <time_frame>within 1 week after CT scanning</time_frame>
    <description>a ratio between the correctly classified pixel and all the classified pixel in one label.</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>First index</measure>
    <time_frame>within 1 week after CT scanning</time_frame>
    <description>(P_(Fat in Affected)+P_(Fibrosis in Affected))/(P_(Fat in Unaffected)+P_(Fibrosis in Unaffected) ) &quot; &quot;</description>
  </secondary_outcome>
  <enrollment type="Actual">27</enrollment>
