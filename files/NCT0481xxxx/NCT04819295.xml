  </study_design_info>
  <primary_outcome>
    <measure>Number of preferences for depression treatment generated from focus groups</measure>
    <time_frame>Baseline</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Number of barriers to depression treatment among Latinx Adolescents from focus groups</measure>
    <time_frame>Baseline</time_frame>
  </secondary_outcome>
  <number_of_groups>3</number_of_groups>
  <enrollment type="Anticipated">70</enrollment>
  <condition>Depression</condition>
