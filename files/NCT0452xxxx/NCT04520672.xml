  <brief_title>Circulating Tumor Cells (CTC) in Cancer</brief_title>
  <official_title>Circulating Tumor Cells (CTC) in Cancer</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>University Hospital, Basel, Switzerland</agency>
