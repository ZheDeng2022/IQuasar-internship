  <brief_title>Phase 2a Study of VX-147 in Adults With APOL1-mediated Focal Segmental Glomerulosclerosis</brief_title>
  <official_title>A Phase 2a, Open-label, Single-arm, 2-Part Study to Evaluate the Efficacy, Safety, and Pharmacokinetics of VX-147 in Adults With APOL1-mediated Focal Segmental Glomerulosclerosis</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Vertex Pharmaceuticals Incorporated</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
  <source>Vertex Pharmaceuticals Incorporated</source>
  <oversight_info>
    <is_fda_regulated_drug>Yes</is_fda_regulated_drug>
    <is_fda_regulated_device>No</is_fda_regulated_device>
  </oversight_info>
  <brief_summary>
    <textblock>
      This study will evaluate the efficacy, safety and pharmacokinetics (PK) of VX-147 in subjects&#xD;
      with apolipoprotein L1 (APOL1)-mediated focal segmental glomerulosclerosis (FSGS).&#xD;
    </textblock>
  </brief_summary>
  <overall_status>Active, not recruiting</overall_status>
  <start_date type="Actual">June 8, 2020</start_date>
  <completion_date type="Anticipated">December 2021</completion_date>
  <primary_completion_date type="Anticipated">December 2021</primary_completion_date>
  <phase>Phase 2</phase>
  <study_type>Interventional</study_type>
  <has_expanded_access>No</has_expanded_access>
  <study_design_info>
    <allocation>N/A</allocation>
    <intervention_model>Single Group Assignment</intervention_model>
    <primary_purpose>Treatment</primary_purpose>
    <masking>None (Open Label)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Percent Change From Baseline in Urine Protein to Creatinine Ratio (UPCR) at Week 13</measure>
    <time_frame>Baseline, Week 13</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Safety and tolerability as assessed by number of subjects with adverse events (AEs) and serious adverse events (SAEs)</measure>
    <time_frame>From Baseline up to Week 17</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Maximum observed concentration (Cmax) of VX-147</measure>
    <time_frame>Pre-dose up to Week 13</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Observed pre-dose concentration (Ctrough) of VX-147</measure>
    <time_frame>Pre-dose</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Area under the concentration versus time curve during a dosing interval (AUCtau) of VX-147</measure>
    <time_frame>Pre-dose up to Week 13</time_frame>
  </secondary_outcome>
  <number_of_arms>1</number_of_arms>
  <enrollment type="Actual">16</enrollment>
  <condition>Glomerulosclerosis, Focal Segmental</condition>
  <arm_group>
    <arm_group_label>VX-147</arm_group_label>
    <arm_group_type>Experimental</arm_group_type>
    <description>Subjects will receive VX-147 orally at Dose 1 for 2 weeks and at Dose 2 for 11 weeks.</description>
  </arm_group>
  <intervention>
    <intervention_type>Drug</intervention_type>
    <intervention_name>VX-147</intervention_name>
    <description>Tablets for oral administration</description>
    <arm_group_label>VX-147</arm_group_label>
  </intervention>
  <eligibility>
    <criteria>
      <textblock>
        Key Inclusion Criteria:&#xD;
&#xD;
          -  APOL1 genotype of G1/G1, G2/G2, or G1/G2&#xD;
&#xD;
          -  FSGS diagnosed by kidney biopsy&#xD;
&#xD;
        Key Exclusion Criteria:&#xD;
&#xD;
          -  Evidence of non-APOL1-mediated FSGS&#xD;
&#xD;
          -  Subjects with known sickle cell disease&#xD;
&#xD;
          -  Solid organ or Bone marrow transplant&#xD;
&#xD;
        Other protocol defined Inclusion/Exclusion criteria may apply.&#xD;
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>65 Years</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>
