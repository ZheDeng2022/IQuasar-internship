  <brief_title>Study in Unsettled Infants Experiencing Feeding Intolerance</brief_title>
  <official_title>Effect of an Experimental Infant Formula in Unsettled Infants Experiencing Feeding Intolerance</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Nestlé</agency>
      <agency_class>Industry</agency_class>
