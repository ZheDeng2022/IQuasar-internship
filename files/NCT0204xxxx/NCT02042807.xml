    <allocation>Randomized</allocation>
    <intervention_model>Parallel Assignment</intervention_model>
    <primary_purpose>Prevention</primary_purpose>
    <masking>Triple (Participant, Care Provider, Investigator)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Cumulative biopsy-detectable prostate cancer rate</measure>
    <time_frame>Week 104 (Month 24)</time_frame>
  </primary_outcome>
  <number_of_arms>3</number_of_arms>
  <enrollment type="Anticipated">702</enrollment>
