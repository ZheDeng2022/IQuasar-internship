  <brief_title>Uterotonics for Severe Preeclampsia</brief_title>
  <official_title>Carbetocin Versus Combined Oxytocin and Misoprostol for Prevention of Postpartum Hemorrhage in Women With Severe Preeclampsia</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Assiut University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
