  <primary_outcome>
    <measure>The rate of postpartum hemorrhage more than 1000 ml</measure>
    <time_frame>30 minutes</time_frame>
    <description>the amount of bleeding during and after CS</description>
  </primary_outcome>
  <number_of_arms>2</number_of_arms>
