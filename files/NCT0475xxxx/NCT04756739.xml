  <brief_title>Menstrual Cycle Symptom Tracking</brief_title>
  <official_title>Menstrual Cycle Symptom Tracking Study</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>SPD Development Company Limited</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
